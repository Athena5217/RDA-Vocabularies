  "@graph": [
    {
      "@id": "http://rdaregistry.info/termList/prodTactile/1004",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "814"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:altLabel": [
        {
          "@language": "fr",
          "@value": "moulage sous vide"
        },
        {
          "@language": "en",
          "@value": "vacuum form"
        }
      ],
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Eine Entstehungsmethode für taktile Ressourcen, bei der ein Kollagen-Master mit einer Plastikfolie bedeckt wird, die erhitzt und abgesaugt wird, um eine Kopie eines Models oder einer Figur zu erzeugen. Auch bekannt als Vakuumformen."
        },
        {
          "@language": "en",
          "@value": "A production method for tactile resources in which a collage master is covered with a sheet of plastic, which is heated and vacuumed to generate a copy of a model or diagram."
        },
        {
          "@language": "fr",
          "@value": "Méthode de production des ressources tactiles où un moule est recouvert d’une feuille de plastique chauffée sous vide pour produire une copie d’une maquette ou d’un diagramme."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/prodTactile"
      },
      "skos:note": {
        "@language": "en",
        "@value": "Also known as: vacuum form."
      },
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "thermoform"
        },
        {
          "@language": "fr",
          "@value": "thermoformage"
        },
        {
          "@language": "es",
          "@value": "termoforma"
        },
        {
          "@language": "de",
          "@value": "Thermoform"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/prodTactile/1003",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "813"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:altLabel": [
        {
          "@language": "fr",
          "@value": "point chaud"
        },
        {
          "@language": "fr",
          "@value": "papier à microcapsules"
        },
        {
          "@language": "en",
          "@value": "microcapsule paper"
        },
        {
          "@language": "en",
          "@value": "hot spot"
        },
        {
          "@language": "en",
          "@value": "Minolta paper"
        },
        {
          "@language": "fr",
          "@value": "papier Minolta"
        }
      ],
      "skos:definition": [
        {
          "@language": "en",
          "@value": "A production method for tactile resources in which an image is printed on a special type of paper with embedded microcapsules of alcohol which burst when exposed to heat to make the surface of the paper swell up."
        },
        {
          "@language": "fr",
          "@value": "Méthode de production des ressources tactiles où une image est imprimée sur un type spécial de papier contenant des microcapsules d’alcool qui éclatent lorsqu’elles sont exposées à la chaleur pour faire gonfler la surface du papier."
        },
        {
          "@language": "de",
          "@value": "Eine Entstehungsmethode für taktile Ressourcen, bei der ein Bild auf ein Spezialpapier gedruckt wird, das Mikrokapseln mit Alkohol enthält, die zerbersten, wenn sie Wärme ausgesetzt werden, und das Papier aufquellen lassen."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/prodTactile"
      },
      "skos:note": {
        "@language": "en",
        "@value": "Also known as: Minolta paper (brand name), microcapsule paper, hot spot."
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "Schwellpapier"
        },
        {
          "@language": "en",
          "@value": "swell paper"
        },
        {
          "@language": "fr",
          "@value": "papier thermogonflé"
        },
        {
          "@language": "es",
          "@value": "papel inflado"
        }
      ],
      "skos:scopeNote": [
        {
          "@language": "fr",
          "@value": "Utilisé pour des dessins tactiles et pour embosser du texte en caractères Moon (Moon linéaire)."
        },
        {
          "@language": "en",
          "@value": "Use for tactile graphics and for embossing text in Moon characters (linear Moon)."
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/prodTactile/1002",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "812"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "en",
          "@value": "A production method for tactile resources in which solid plastic dots are heat sealed onto the surface of thin but strong paper."
        },
        {
          "@language": "fr",
          "@value": "Méthode de production des ressources tactiles par laquelle des points en plastique dur sont thermocollés à la surface d’un papier fin mais résistant."
        },
        {
          "@language": "de",
          "@value": "Eine Entstehungsmethode für taktile Ressourcen, bei der Fest-Kunststoff-Punkte auf der Oberfläche von dünnem, aber festem Papier heißversiegelt werden. "
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/prodTactile"
      },
      "skos:prefLabel": [
        {
          "@language": "fr",
          "@value": "point rigide"
        },
        {
          "@language": "es",
          "@value": "punto sólido"
        },
        {
          "@language": "en",
          "@value": "solid dot"
        },
        {
          "@language": "de",
          "@value": "Thermodruck"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/prodTactile/1001",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "811"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Eine Entstehungsmethode für taktile Ressourcen, bei der entweder eine Metall- oder eine Kunststoffplatte als Master bzw. Druckvorlage verwendet wird, um Braille- und Moonschrift-Kopien zu erstellen (häufig in Kombination mit einem Computer und spezialisierter Software)."
        },
        {
          "@language": "fr",
          "@value": "Méthode de production des ressources tactiles utilisant une feuille de métal ou de plastique comme matrice ou équipement d’embossage (souvent combiné avec un ordinateur et un logiciel spécialisé) pour produire des exemplaires en braille et Moon."
        },
        {
          "@language": "en",
          "@value": "A production method for tactile resources using either a metal or plastic sheet as a master or embossing equipment (often in combination with a computer and specialized software) to produce braille and Moon copies."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/prodTactile"
      },
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "embossed"
        },
        {
          "@language": "de",
          "@value": "geprägt"
        },
        {
          "@language": "es",
          "@value": "repujado"
        },
        {
          "@language": "fr",
          "@value": "embossage"
        }
      ],
      "skos:scopeNote": [
        {
          "@language": "en",
          "@value": "Use for \"plate copy\" or \"press braille\" or braille generated using Braillo equipment."
        },
        {
          "@language": "en",
          "@value": "Also use for \"dotty Moon\" (where the Moon characters are embossed as lines of dots), including use of Tiger Embosser equipment."
        }
      ]
    },
    {
      "@id": "http://metadataregistry.org/uri/RegStatus/1001",
      "@type": "skos:Concept",
      "skos:prefLabel": {
        "@language": "en",
        "@value": "Published"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/prodTactile",
      "@type": "skos:ConceptScheme",
      "dc:title": "RDA Production Method for Tactile Resource"
    }
  ]
