        {
            "@id": "http://rdaregistry.info/termList/extentCarto/1001",
            "@type": "Concept",
            "api": "http://api.metadataregistry.org/concepts/761",
            "definition": {
                "en": "A unit of extent of cartographic resource that is a volume of maps or other cartographic content, with or without descriptive text. (Deprecated)"
            },
            "inScheme": "http://rdaregistry.info/termList/extentCarto",
            "prefLabel": {
