{
  "@context": {
    "dc": "http://purl.org/dc/elements/1.1/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "reg": "http://metadataregistry.org/uri/schema/registry/",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "xsd": "http://www.w3.org/2001/XMLSchema#"
  },
  "@graph": [
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType/1008",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "482"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "en",
          "@value": "Media used to store moving or still images, designed for use with a playback device such as a videocassette player or DVD player. "
        },
        {
          "@language": "de",
          "@value": "Medien, die für die Speicherung von bewegten oder einzelnen Bildern verwendet werden, und die für die Benutzung mit einem Wiedergabegerät wie einem Videokassetten- oder DVD-Player konzipiert wurden."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAMediaType"
      },
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "video"
        },
        {
          "@language": "de",
          "@value": "Videomaterial"
        }
      ],
      "skos:scopeNote": [
        {
          "@language": "de",
          "@value": "Dazu gehören sowohl Medien für die Speicherung von digital kodierten als auch von analogen Bildern. "
        },
        {
          "@language": "en",
          "@value": "Includes media used to store digitally encoded as well as analog images."
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType/1005",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "479"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Medien, die für die Speicherung von bewegten oder stehenden Bildern verwendet werden, und die für die Benutzung mit einem Projektionsgerät wie einem Film-, Dia- oder Overhead-Projektor konzipiert wurden."
        },
        {
          "@language": "en",
          "@value": "Media used to store moving or still images, designed for use with a projection device such as a motion picture film projector, slide projector, or overhead projector.  "
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAMediaType"
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "projizierbare Medien"
        },
        {
          "@language": "en",
          "@value": "projected"
        }
      ],
      "skos:scopeNote": [
        {
          "@language": "de",
          "@value": "Dazu gehören sowohl Medien für die Projektion zweidimensionaler als auch dreidimensionaler Bilder."
        },
        {
          "@language": "en",
          "@value": "Includes media designed to project both two-dimensional and three-dimensional images."
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType/1004",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "478"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Medien, die zur Speicherung winziger Objekte verwendet werden, die für die Benutzung mithilfe eines Geräts wie ein Mikroskop konzipiert wurden, das Details erkennen lässt, die für das bloße Auge nicht sichtbar sind. "
        },
        {
          "@language": "en",
          "@value": "Media used to store minute objects, designed for use with a device such as a microscope to reveal details invisible to the naked eye."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAMediaType"
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "mikroskopische Medien"
        },
        {
          "@language": "en",
          "@value": "microscopic"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType/1007",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "481"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Medien, die für die Speicherung von Inhalten verwendet werden, die ohne Hilfsmittel direkt durch einen oder mehreren der menschlichen Sinne wahrgenommen werden können."
        },
        {
          "@language": "en",
          "@value": "Media used to store content designed to be perceived directly through one or more of the human senses without the aid of an intermediating device. "
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAMediaType"
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "ohne Hilfsmittel zu benutzende Medien"
        },
        {
          "@language": "en",
          "@value": "unmediated"
        }
      ],
      "skos:scopeNote": [
        {
          "@language": "de",
          "@value": "Dazu gehören Medien mit visuellem und/oder taktilem Inhalt, die mit Verfahren wie Druck, Gravur, Lithografie usw. oder Prägung, Texturierung usw. oder durch Handschrift, Zeichnen, Malen usw. hergestellt werden."
        },
        {
          "@language": "en",
          "@value": "Also includes media used to convey three-dimensional forms such as sculptures, models, etc."
        },
        {
          "@language": "de",
          "@value": "Dazu gehören auch Medien, die zur Vermittlung dreidimensionaler Formen verwendet werden wie Skulpturen, Modelle usw."
        },
        {
          "@language": "en",
          "@value": "Includes media containing visual and/or tactile content produced using processes such as printing, engraving, lithography, etc., embossing, texturing, etc., or by means of handwriting, drawing, painting, etc.  "
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType/1006",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "480"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Medien, die für die Speicherung von Stereo-Bildpaaren verwendet werden, die für die Betrachtung mit einem Gerät wie einem Stereoskop oder einem stereografischen Betrachter konzipiert wurden, um  den Eindruck von Dreidimensionalität zu erzeugen. "
        },
        {
          "@language": "en",
          "@value": "Media used to store pairs of still images, designed for use with a device such as a stereoscope or stereograph viewer to give the effect of three dimensions."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAMediaType"
      },
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "stereographic"
        },
        {
          "@language": "de",
          "@value": "stereoskopische Medien"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType/1001",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "475"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Medien, die zur Speicherung von aufgenommenen Ton verwendet werden, und die mit Wiedergabegeräten wie Plattenspieler, Audiokassetten-Abspielgerät, CD-Player oder MP3-Player abgespielt werden."
        },
        {
          "@language": "en",
          "@value": "Media used to store recorded sound, designed for use with a playback device such as a turntable, audiocassette player, CD player, or MP3 player. "
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAMediaType"
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "auditive Medien"
        },
        {
          "@language": "en",
          "@value": "audio"
        }
      ],
      "skos:scopeNote": [
        {
          "@language": "en",
          "@value": "Includes media used to store digitally encoded as well as analog sound."
        },
        {
          "@language": "de",
          "@value": "Dazu gehören sowohl Medien für die Speicherung von digital kodiertem als auch von analogem Ton."
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType/1003",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "477"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "en",
          "@value": "Media used to store electronic files, designed for use with a computer. "
        },
        {
          "@language": "de",
          "@value": "Medien, die zur Speicherung von elektronischen Dateien verwendet werden, die für die Computeranwendung konzipiert wurden."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAMediaType"
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "Computermedien"
        },
        {
          "@language": "en",
          "@value": "computer"
        }
      ],
      "skos:scopeNote": [
        {
          "@language": "de",
          "@value": "Dazu gehören sowohl Medien, die per Fernzugriff über Dateiserver aufgerufen werden als auch Medien im Direktzugriff, wie Computerbänder, Disketten, CD-ROMs und Festplatten."
        },
        {
          "@language": "en",
          "@value": "Includes media that are accessed remotely through file servers as well as direct-access media such as computer tapes and discs."
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType/1002",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "476"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Medien, die zur Speicherung von verkleinerten Bildern verwendet werden, die für das menschliche Auge nicht lesbar sind und für die Benutzung mithilfe von Geräten wie Mikrofilm- oder Mikrofiche-Lesegeräten konzipiert wurden."
        },
        {
          "@language": "en",
          "@value": "Media used to store reduced-size images not readable to the human eye, designed for use with a device such as a microfilm or microfiche reader."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAMediaType"
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "Mikroformen"
        },
        {
          "@language": "en",
          "@value": "microform"
        }
      ],
      "skos:scopeNote": [
        {
          "@language": "en",
          "@value": "Includes both transparent and opaque micrographic media."
        },
        {
          "@language": "de",
          "@value": "Dazu gehören sowohl transparente als auch llichtundurchlässige, mikrografische Medien."
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType",
      "@type": "skos:ConceptScheme",
      "dc:title": "RDA Media Type"
    },
    {
      "@id": "http://metadataregistry.org/uri/RegStatus/1001",
      "@type": "skos:Concept",
      "skos:prefLabel": {
        "@language": "en",
        "@value": "Published"
      }
    }
  ]
}