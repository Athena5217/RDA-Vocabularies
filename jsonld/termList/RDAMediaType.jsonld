{
  "@context": {
    "dc": "http://purl.org/dc/elements/1.1/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "reg": "http://metadataregistry.org/uri/schema/registry/",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "xsd": "http://www.w3.org/2001/XMLSchema#"
  },
  "@graph": [
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType",
      "@type": "skos:ConceptScheme",
      "dc:title": "RDA Media Type"
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType/1008",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "482"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "en",
          "@value": "Media used to store moving or still images, designed for use with a playback device such as a videocassette player or DVD player. "
        },
        {
          "@language": "de",
          "@value": "Medien, die zur Speicherung von bewegten oder unbewegten Bildern verwendet werden, und die für die Benutzung mit einem Wiedergabegerät wie zum Beispiel einem Videokassetten- oder DVD-Player konzipiert sind."
        },
        {
          "@language": "fr",
          "@value": "Média servant à emmagasiner des images animées ou fixes et conçu pour une utilisation au moyen d’un dispositif de lecture tel qu’un lecteur de cassette vidéo ou de DVD."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAMediaType"
      },
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "video"
        },
        {
          "@language": "fr",
          "@value": "vidéo"
        },
        {
          "@language": "de",
          "@value": "video"
        }
      ],
      "skos:scopeNote": [
        {
          "@language": "fr",
          "@value": "Comprend tout média servant à emmagasiner des images encodées numériquement aussi bien que des images analogiques."
        },
        {
          "@language": "en",
          "@value": "Includes media used to store digitally encoded as well as analog images."
        },
        {
          "@language": "de",
          "@value": "Dazu gehören sowohl Medien für die Speicherung von digital kodierten als auch von analogen Bildern."
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType/1002",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "476"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "en",
          "@value": "Media used to store reduced-size images not readable to the human eye, designed for use with a device such as a microfilm or microfiche reader."
        },
        {
          "@language": "fr",
          "@value": "Média servant à emmagasiner des images de taille réduite non lisibles à l’œil nu, et conçu pour une utilisation au moyen d’un dispositif tel que : lecteur de microfilm ou de microfiche."
        },
        {
          "@language": "de",
          "@value": "Medien, die zur Speicherung von verkleinerten Bildern verwendet werden, die für das menschliche Auge nicht lesbar sind und die für die Benutzung mithilfe von Geräten wie z. B. Mikrofilm- oder Mikrofiche-Lesegeräten konzipiert sind."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAMediaType"
      },
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "microform"
        },
        {
          "@language": "fr",
          "@value": "microforme"
        },
        {
          "@language": "de",
          "@value": "Mikroform"
        }
      ],
      "skos:scopeNote": [
        {
          "@language": "en",
          "@value": "Includes both transparent and opaque micrographic media."
        },
        {
          "@language": "fr",
          "@value": "Comprend les deux médias microphotographiques, transparent et opaque."
        },
        {
          "@language": "de",
          "@value": "Dazu gehören sowohl transparente als auch lichtundurchlässige, mikrografische Medien."
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType/1004",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "478"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "en",
          "@value": "Media used to store minute objects, designed for use with a device such as a microscope to reveal details invisible to the naked eye."
        },
        {
          "@language": "de",
          "@value": "Medien, die zur Speicherung winziger Objekte verwendet werden, die für die Benutzung mithilfe eines Geräts wie z. B. ein Mikroskop konzipiert sind, das Details erkennen lässt, die für das bloße Auge nicht sichtbar sind. "
        },
        {
          "@language": "fr",
          "@value": "Média servant à emmagasiner des objets minuscules et conçu pour une utilisation au moyen d’un dispositif tel qu’un microscope, pour faire apparaître des détails invisibles à l’œil nu."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAMediaType"
      },
      "skos:prefLabel": [
        {
          "@language": "fr",
          "@value": "microscopique"
        },
        {
          "@language": "en",
          "@value": "microscopic"
        },
        {
          "@language": "de",
          "@value": "mikroskopisch"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType/1005",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "479"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "fr",
          "@value": "Média servant à emmagasiner des images, animées ou fixes, et conçu pour une utilisation au moyen d’un dispositif de projection tel que : projecteur de film cinématographique, projecteur de diapositives ou rétroprojecteur."
        },
        {
          "@language": "en",
          "@value": "Media used to store moving or still images, designed for use with a projection device such as a motion picture film projector, slide projector, or overhead projector.  "
        },
        {
          "@language": "de",
          "@value": "Medien, die zur Speicherung von bewegten oder unbewegten Bildern verwendet werden, die für die Benutzung mit einem Projektionsgerät wie z. B. einem Film-, Dia- oder Overhead-Projektor konzipiert sind."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAMediaType"
      },
      "skos:prefLabel": [
        {
          "@language": "fr",
          "@value": "projeté"
        },
        {
          "@language": "en",
          "@value": "projected"
        },
        {
          "@language": "de",
          "@value": "projizierbar"
        }
      ],
      "skos:scopeNote": [
        {
          "@language": "de",
          "@value": "Dazu gehören sowohl Medien für die Projektion zweidimensionaler als auch dreidimensionaler Bilder."
        },
        {
          "@language": "en",
          "@value": "Includes media designed to project both two-dimensional and three-dimensional images."
        },
        {
          "@language": "fr",
          "@value": "Comprend tout média conçu pour projeter des images bidimensionnelles et tridimensionnelles."
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType/1006",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "480"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Medien, die zur Speicherung von Paaren von unbewegten Bildern verwendet werden, die für die Betrachtung mit einem Gerät wie z. B. einem Stereoskop oder einem stereografischen Betrachter konzipiert sind, um den Eindruck von Dreidimensionalität zu erzeugen."
        },
        {
          "@language": "en",
          "@value": "Media used to store pairs of still images, designed for use with a device such as a stereoscope or stereograph viewer to give the effect of three dimensions."
        },
        {
          "@language": "fr",
          "@value": "Média servant à emmagasiner des paires d’images fixes et conçu pour une utilisation au moyen d’un dispositif tel qu’un stéréoscope ou une visionneuse stéréoscopique pour donner l’impression des trois dimensions."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAMediaType"
      },
      "skos:prefLabel": [
        {
          "@language": "fr",
          "@value": "stéréoscopique"
        },
        {
          "@language": "de",
          "@value": "stereografisch"
        },
        {
          "@language": "en",
          "@value": "stereographic"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType/1007",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "481"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "en",
          "@value": "Media used to store content designed to be perceived directly through one or more of the human senses without the aid of an intermediating device. "
        },
        {
          "@language": "fr",
          "@value": "Média servant à emmagasiner un contenu conçu pour être perçu directement par un ou plusieurs sens humains sans l’aide d’un dispositif de médiation."
        },
        {
          "@language": "de",
          "@value": "Medien, die zur Speicherung von Inhalten verwendet werden, die ohne Hilfsmittel direkt durch einen oder mehrere der menschlichen Sinne wahrgenommen werden können."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAMediaType"
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "ohne Hilfsmittel zu benutzen"
        },
        {
          "@language": "en",
          "@value": "unmediated"
        },
        {
          "@language": "fr",
          "@value": "sans médiation"
        }
      ],
      "skos:scopeNote": [
        {
          "@language": "en",
          "@value": "Includes media containing visual and/or tactile content produced using processes such as printing, engraving, lithography, etc., embossing, texturing, etc., or by means of handwriting, drawing, painting, etc. Also includes media used to convey three-dimensional forms such as sculptures, models, etc."
        },
        {
          "@language": "fr",
          "@value": "Comprend tout média à contenu visuel et/ou tactile produit selon des procédés tels que : impression, gravure, lithographie, etc. ; embossage, texturation, etc. ; ou bien encore au moyen de l’écriture à la main, du dessin, de la peinture, etc. Comprend également tout média servant à communiquer des formes tridimensionnelles telles que : sculptures, maquettes, etc."
        },
        {
          "@language": "en",
          "@value": "Also includes media used to convey three-dimensional forms such as sculptures, models, etc."
        },
        {
          "@language": "de",
          "@value": "Dazu gehören Medien mit visuellem und/oder taktilem Inhalt, die mit bestimmten Verfahren wie z. B. Druck, Stich, Lithografie usw. oder Prägung, Texturierung usw. oder durch Handschrift, Zeichnen, Malen usw. hergestellt werden. Dazu gehören auch Medien, die dazu verwendet werden, dreidimensionale Formen wie Skulpturen oder Modelle usw. auszudrücken."
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType/1001",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "475"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Medien, die zur Speicherung von aufgenommenem Ton verwendet werden, und die für die Benutzung mit einem Wiedergabegerät wie z. B. Plattenspieler, Audiokassetten-Player, CD-Player oder MP3-Player konzipiert sind."
        },
        {
          "@language": "en",
          "@value": "Media used to store recorded sound, designed for use with a playback device such as a turntable, audiocassette player, CD player, or MP3 player. "
        },
        {
          "@language": "fr",
          "@value": "Média servant à emmagasiner du son enregistré et conçu pour une utilisation au moyen d’un dispositif de lecture tel qu’une platine, un lecteur de cassette audio, un lecteur de CD ou un lecteur MP3."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAMediaType"
      },
      "skos:prefLabel": [
        {
          "@language": "fr",
          "@value": "audio"
        },
        {
          "@language": "en",
          "@value": "audio"
        },
        {
          "@language": "de",
          "@value": "audio"
        }
      ],
      "skos:scopeNote": [
        {
          "@language": "en",
          "@value": "Includes media used to store digitally encoded as well as analog sound."
        },
        {
          "@language": "de",
          "@value": "Dazu gehören sowohl Medien für die Speicherung von digital kodiertem als auch von analogem Ton."
        },
        {
          "@language": "fr",
          "@value": "Comprend tout média servant à emmagasiner du son encodé numériquement aussi bien que du son analogique."
        }
      ]
    },
    {
      "@id": "http://metadataregistry.org/uri/RegStatus/1001",
      "@type": "skos:Concept",
      "skos:prefLabel": {
        "@language": "en",
        "@value": "Published"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAMediaType/1003",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "477"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Medien, die zur Speicherung von elektronischen Dateien verwendet werden, die für die Computeranwendung konzipiert sind."
        },
        {
          "@language": "en",
          "@value": "Media used to store electronic files, designed for use with a computer. "
        },
        {
          "@language": "fr",
          "@value": "Média servant à emmagasiner des fichiers électroniques et conçu pour une utilisation au moyen d’un ordinateur."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAMediaType"
      },
      "skos:prefLabel": [
        {
          "@language": "fr",
          "@value": "informatique"
        },
        {
          "@language": "en",
          "@value": "computer"
        },
        {
          "@language": "de",
          "@value": "Computermedien"
        }
      ],
      "skos:scopeNote": [
        {
          "@language": "en",
          "@value": "Includes media that are accessed remotely through file servers as well as direct-access media such as computer tapes and discs."
        },
        {
          "@language": "de",
          "@value": "Dazu gehören sowohl Medien, die per Fernzugriff über Dateiserver aufgerufen werden als auch Medien im Direktzugriff, wie z. B. Computerbänder, Disketten, CD-ROMs und Festplatten."
        },
        {
          "@language": "fr",
          "@value": "Comprend tout média accessible à distance par serveur aussi bien que tout média en accès direct tel que des bandes et des disques informatiques."
        }
      ]
    }
  ]
}