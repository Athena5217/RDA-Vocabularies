    {
      "@id": "http://rdaregistry.info/termList/MusNotation/1007",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "705"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "en",
          "@value": "A system of musical notation in wide use for Western art music, conveying pitch and duration using a staff of multiple parallel lines (in 15th- to 21st-century music, usually five lines) often in combination with other staves."
        },
        {
          "@language": "fr",
          "@value": "Système de notation musicale largement utilisé pour la musique occidentale, indiquant hauteurs et durées au moyen d’une portée de plusieurs lignes parallèles (dans la musique du 15e au 21e siècle, ordinairement cinq lignes) souvent combinée avec d’autres portées."
        },
        {
          "@language": "de",
          "@value": "Ein System der Musiknotation im Einsatz für westliche Musik, das die Tonlage und Dauer durch mehrere parallele Notenlinien ausdrückt (in der Musik des 15. bis 21. Jahrhunderts normalerweise fünf Linien), häufig in Kombination mit anderen Notenlinien."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/MusNotation"
      },
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "staff notation"
        },
        {
          "@language": "de",
          "@value": "Liniennotation"
        },
        {
          "@language": "fr",
          "@value": "notation sur portée"
        }
      ]
    },
    {
      "@id": "http://metadataregistry.org/uri/RegStatus/1008",
      "@type": "skos:Concept",
      "skos:prefLabel": {
        "@language": "en",
        "@value": "Deprecated"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/MusNotation/1006",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "704"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "en",
          "@value": "The designation of pitches by means of conventional syllables rather than letter names."
        },
        {
          "@language": "de",
          "@value": "Die Kennzeichnung von Tonhöhen durch konventionelle Silben anstelle von Buchstabenbezeichnungen "
        },
        {
          "@language": "fr",
          "@value": "La désignation des hauteurs de sons par des syllabes de convention plutôt que par des lettres."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/MusNotation"
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "Solmisation"
        },
        {
          "@language": "en",
          "@value": "solmization"
        },
        {
          "@language": "fr",
          "@value": "solmisation"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/MusNotation/1010",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "1036"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "fr",
          "@value": "Système de notation musicale utilisant des neumes, c’est-à-dire des signes graphiques qui représentent essentiellement le mouvement d’une ligne mélodique."
        },
        {
          "@language": "en",
          "@value": "A system of musical notation using neumes, i.e., graphic signs that represent essentially the movement in pitch of a melody."
        },
        {
          "@language": "de",
          "@value": "Ein System der Musiknotation, das Neumen verwendet, d. h. grafische Zeichen, die die Tonhöhenbewegung einer Melodie darstellen."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/MusNotation"
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "Neumennotation"
        },
        {
          "@language": "en",
          "@value": "neumatic notation"
        },
        {
          "@language": "fr",
          "@value": "notation neumatique"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/MusNotation/1004",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "702"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "en",
          "@value": "A system of musical notation conveying pitch by use of numbers, assigned to the notes of a scale, the keys of a keyboard, the finger positions or frets of a string instrument, or to the holes or valves of a wind instrument."
        },
        {
          "@language": "fr",
          "@value": "Système de notation musicale indiquant les hauteurs de sons au moyen de nombres, affectés aux notes d’une gamme, aux touches d’un clavier, aux positions des doigts ou aux frettes d’un instrument à cordes, ou bien encore aux trous ou aux pistons d’un instrument à vent."
        },
        {
          "@language": "de",
          "@value": "Ein System der Musiknotation, das die Tonhöhe durch die Verwendung von Zahlen ausdrückt, die den Noten einer Skala, den Tasten eines Keybords, den Fingerpositionen oder den Bünden eines Saiteninstruments oder den Löchern bzw. Ventilen eines Blasinstruments zugeordnet sind."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/MusNotation"
      },
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "number notation"
        },
        {
          "@language": "de",
          "@value": "Ziffernnotation"
        },
        {
          "@language": "fr",
          "@value": "notation chiffrée"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/MusNotation/1005",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "703"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1008"
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/MusNotation"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "plainsong notation"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/MusNotation/1002",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "700"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "en",
          "@value": "Musical notation that uses the letters of the alphabet to designate pitches."
        },
        {
          "@language": "fr",
          "@value": "Notation musicale qui utilise les lettres de l’alphabet pour désigner les hauteurs de sons."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/MusNotation"
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "Tonnamen"
        },
        {
          "@language": "en",
          "@value": "letter notation"
        },
        {
          "@language": "fr",
          "@value": "notation alphabétique"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/MusNotation/1003",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "701"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Ein System zur Notation der Tondauer, das um 1260 aufgekommen ist und bis ca. 1600 gedauert hat und die vier Hauptnoten-Werte und die damit verbundenen Pausen verwendet: longa, brevis, semi-brevis und minima."
        },
        {
          "@language": "fr",
          "@value": "Système de notation de la durée, ayant débuté aux environs de 1260 et s’étant prolongé jusque vers 1600, qui utilisait quatre valeurs de notes principales et les silences associés : longue, brève, semi-brève et minime."
        },
        {
          "@language": "en",
          "@value": "A system of notating duration, beginning around 1260 and continuing through about 1600, employing four principal note-values and associated rests: long, breve, semibreve, and minim."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/MusNotation"
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "Mensuralnotation"
        },
        {
          "@language": "en",
          "@value": "mensural notation"
        },
        {
          "@language": "fr",
          "@value": "notation proportionnelle"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/MusNotation/1001",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "699"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "fr",
          "@value": "Forme de notation musicale qui utilise différentes lignes suggestives, des symboles, de la couleur, etc. pour aider ou guider les interprètes. Elle est utilisée pour de la musique dont les hauteurs, la durée, le tempérament, etc. ne sont pas déterminés ainsi que pour décrire de la musique électronique où aucun interprète n’est impliqué."
        },
        {
          "@language": "de",
          "@value": "Eine Form von Musiknotation, die verschiedene suggestive Linien, Symbole, Farben usw. verwendet, um die Ausführenden anzuregen oder zu führen. Sie wird für Musik, die unbestimmt in Tonhöhe, Dauer, Stimmung usw. ist, und auch zur Darstellung elektronischer Musik, bei der kein Ausführender beteiligt ist, verwendet."
        },
        {
          "@language": "en",
          "@value": "A form of musical notation that uses various suggestive lines, symbols, colour, etc., to prompt or guide the performers. It is used for music that is indeterminate in pitch, duration, temperament, etc., and also to depict electronic music in which no performer is involved."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/MusNotation"
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "Grafische Notation"
        },
        {
          "@language": "fr",
          "@value": "notation graphique"
        },
        {
          "@language": "en",
          "@value": "graphic notation"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/MusNotation",
      "@type": "skos:ConceptScheme",
      "dc:title": "RDA Form of Musical Notation"
    },
    {
      "@id": "http://rdaregistry.info/termList/MusNotation/1008",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "706"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Jedes Notationsystem ab dem Jahr 1300, das Buchstaben, Ziffern und andere Zeichen als Alternative zur konventionellen Liniennotation verwendet."
        },
        {
          "@language": "fr",
          "@value": "Tout système de notation utilisé à partir de 1300, utilisant des lettres, des chiffres ou d’autres signes comme alternative à la notation conventionnelle sur portée."
        },
        {
          "@language": "en",
          "@value": "Any notational system from 1300 or later that uses letters, numerals, or other signs as an alternative to conventional staff notation."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/MusNotation"
      },
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "tablature"
        },
        {
          "@language": "de",
          "@value": "Tabulatur"
        },
        {
          "@language": "fr",
          "@value": "tablature"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/MusNotation/1009",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "707"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "fr",
          "@value": "Système de notation musicale qui remplace la notation sur portée par les syllabes sol-fa ou leurs initiales."
        },
        {
          "@language": "en",
          "@value": "A system of musical notation that replaces staff notation with sol-fa syllables or their initials.."
        },
        {
          "@language": "de",
          "@value": "Ein System zur Musiknotation, das die Liniennotation durch Sol-Fa-Silben oder ihre Initialen ersetzt."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/MusNotation"
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "Tonic Sol-Fa"
        },
        {
          "@language": "en",
          "@value": "tonic sol-fa"
        },
        {
          "@language": "fr",
          "@value": "tonic sol-fa"
        }
      ]
    }
  ]
