      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Ein System zur Musiknotation, das die Liniennotation durch Sol-Fa-Silben oder ihre Initialen ersetzt."
        },
        {
          "@language": "fr",
          "@value": "Système de notation musicale qui remplace la notation sur portée par les syllabes sol-fa ou leurs initiales."
        },
        {
          "@language": "es",
          "@value": "Sistema de notación musical que reemplaza la notación pautada con sílabas sol-fa o sus iniciales."
        },
        {
          "@language": "en",
          "@value": "A system of musical notation that replaces staff notation with sol-fa syllables or their initials.."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/MusNotation"
      },
      "skos:prefLabel": [
        {
          "@language": "fr",
          "@value": "tonic sol-fa"
        },
        {
          "@language": "de",
          "@value": "Tonic Sol-Fa"
        },
        {
          "@language": "es",
          "@value": "tónica sol-fa"
        },
        {
          "@language": "en",
          "@value": "tonic sol-fa"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/MusNotation/1008",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "706"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Jedes Notationsystem ab dem Jahr 1300, das Buchstaben, Ziffern und andere Zeichen als Alternative zur konventionellen Liniennotation verwendet."
        },
        {
          "@language": "fr",
          "@value": "Tout système de notation utilisé à partir de 1300, utilisant des lettres, des chiffres ou d’autres signes comme alternative à la notation conventionnelle sur portée."
        },
        {
          "@language": "es",
          "@value": "Sistema notacional de 1300 o posterior, que utiliza letras, números u otros signos como alternativa a la notación pautada convencional."
        },
        {
          "@language": "en",
          "@value": "Any notational system from 1300 or later that uses letters, numerals, or other signs as an alternative to conventional staff notation."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/MusNotation"
