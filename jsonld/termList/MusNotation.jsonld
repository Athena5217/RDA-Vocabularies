      },
      "skos:definition": {
        "@language": "en",
        "@value": "A system of musical notation in wide use for Western art music, conveying pitch and duration using a staff of multiple parallel lines (in 15th- to 21st-century music, usually five lines) often in combination with other staves."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/MusNotation"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "staff notation"
      }
    },
