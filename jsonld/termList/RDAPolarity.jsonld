  "@graph": [
    {
      "@id": "http://rdaregistry.info/termList/RDAPolarity/1003",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "485"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Eine Polarität, bei der die Farben und Tönungen, die für Bilder verwendet werden eine Mischung derjenigen sind, die mit dem menschlichen Auge gesehen werden und denen, die dem entgegengesetzt sind, was das menschliche Auge sehen würde."
        },
        {
          "@language": "fr",
          "@value": "Eine Polarität, bei der die Farben und Tönungen, die für Bilder verwendet werden eine Mischung derjenigen sind, die mit dem menschlichen Auge gesehen werden und denen, die dem entgegengesetzt sind, was das menschliche Auge sehen würde."
        },
        {
          "@language": "en",
          "@value": "A polarity in which colours and tones used for images are a mixture of those seen with the human eye and those opposite what the human eye would see."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAPolarity"
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "gemischte Polarität"
        },
        {
          "@language": "en",
          "@value": "mixed polarity"
        },
        {
          "@language": "fr",
          "@value": "polarités diverses"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAPolarity/1002",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "484"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Eine Polarität, bei der die Farben und Tönungen, die für Bilder verwendet werden, denen entgegengesetzt sind, die mit dem menschlichen Auge gesehen werden."
        },
        {
          "@language": "en",
          "@value": "A polarity in which colours and tones used for images are opposite those seen with the human eye."
        },
        {
          "@language": "fr",
          "@value": "Polarité dans laquelle les couleurs et les tons employés pour les images sont inversés par rapport à ce que verrait l’oeil humain."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAPolarity"
      },
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "negative"
        },
        {
          "@language": "fr",
          "@value": "négatif"
        },
        {
          "@language": "de",
          "@value": "negativ"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAPolarity/1001",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "483"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Eine Polarität, bei der die Farben und Tönungen, die für Bilder verwendet werden, dieselben sind, wie diejenigen, die mit dem menschlichen Auge gesehen werden."
        },
        {
          "@language": "en",
          "@value": "A polarity in which colours and tones used for images are the same as those seen with the human eye."
        },
        {
          "@language": "fr",
          "@value": "Polarité dans laquelle les couleurs et les tons employés pour les images sont identiques à ceux vus par l’oeil humain."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAPolarity"
      },
      "skos:prefLabel": [
        {
          "@language": "fr",
          "@value": "positif"
        },
        {
          "@language": "en",
          "@value": "positive"
        },
        {
          "@language": "de",
          "@value": "positiv"
        }
      ]
    },
