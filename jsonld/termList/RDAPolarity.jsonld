        {
          "@language": "es",
          "@value": "negativa"
        },
        {
          "@language": "fr",
          "@value": "négatif"
        },
        {
          "@language": "de",
          "@value": "negativ"
        },
        {
          "@language": "en",
          "@value": "negative"
        }
      ]
    },
