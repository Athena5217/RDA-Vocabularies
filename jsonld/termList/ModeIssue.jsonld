  },
  "@graph": [
    {
      "@id": "http://rdaregistry.info/termList/ModeIssue/1004",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "744"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A resource that is added to or changed by means of updates that do not remain discrete and are integrated into the whole. An integrating resource may be tangible (e.g., a loose-leaf manual that is updated by means of replacement pages) or intangible (e.g., a Web site that is updated either continuously or on a cyclical basis)."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/ModeIssue"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "integrating resource"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/ModeIssue/1001",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "741"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A resource that is issued either as a single physical unit (e.g., as a single-volume monograph) or, in the case of an intangible resource, as a single logical unit (e.g., as a PDF file mounted on the Web)."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/ModeIssue"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "single unit"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/ModeIssue/1003",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "743"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A resource issued in successive parts, usually bearing numbering, that has no predetermined conclusion (e.g., a periodical, a monographic series, a newspaper)."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/ModeIssue"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "serial"
      },
      "skos:scopeNote": {
        "@language": "en",
        "@value": "Includes resources that exhibit characteristics of serials, such as successive issues, numbering, and frequency, but whose duration is limited (e.g., newsletters of events) and reproductions of serials."
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/ModeIssue/1002",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "742"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A resource issued in two or more parts (either simultaneously or successively) that is complete or intended to be completed within a finite number of parts (e.g., a dictionary in two volumes, three audiocassettes issued as a set)."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/ModeIssue"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "multipart monograph"
      }
    },
    {
      "@id": "http://metadataregistry.org/uri/RegStatus/1001",
      "@type": "skos:Concept",
      "skos:prefLabel": {
        "@language": "en",
        "@value": "Published"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/ModeIssue",
      "@type": "skos:ConceptScheme",
      "dc:title": "RDA Mode of Issuance"
    }
  ]
}