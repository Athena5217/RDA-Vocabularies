  "@graph": [
    {
      "@id": "http://rdaregistry.info/termList/noteMove/1005",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "1031"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "en",
          "@value": "A form of notated movement using a spherical coordinate system to denote body and limb positions. It is recorded on a grid that represents the body."
        },
        {
          "@language": "de",
          "@value": "Eine Form der Bewegungsnotation, die ein sphärisches Koordinatensystem verwendet, um die Positionen des Körpers und der Gliedmaßen anzuzeigen. Sie wird auf einem Gitternetz erfasst, das den Körper darstellt."
        },
        {
          "@language": "fr",
          "@value": "Forme de notation du mouvement qui emploie un système sphérique de coordonnées pour représenter les positions du corps et des membres. Le système utilise une grille qui représente le corps."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/noteMove"
      },
      "skos:prefLabel": [
        {
          "@language": "fr",
          "@value": "notation Eshkol-Wachman"
        },
        {
          "@language": "de",
          "@value": "Eshkol-Wachmann-Notation"
        },
        {
          "@language": "en",
          "@value": "Eshkol-Wachman movement notation"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/noteMove/1004",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "1030"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Eine Form der Bewegungsnotation, die taktische Bewegungen während eines Spiels wie zum Beispiel Schach aufzeichnet."
        },
        {
          "@language": "en",
          "@value": "A form of notated movement recording tactical movements during the course of a game, such as chess."
        },
        {
          "@language": "fr",
          "@value": "Forme de notation du mouvement servant à enregistrer les mouvements tactiques pendant le déroulement d’un jeu, comme les échecs."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/noteMove"
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "Spielnotation"
        },
        {
          "@language": "en",
          "@value": "game play notation"
        },
        {
          "@language": "fr",
          "@value": "notation de jeu"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/noteMove/1007",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "1033"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "en",
          "@value": "A form of notated movement using abstract symbols to represent body and limb positions as seen from the back. It is recorded on a five–line horizontal staff that represents the body."
        },
        {
          "@language": "fr",
          "@value": "Forme de notation du mouvement qui emploie des symboles abstraits pour représenter les positions du corps et des membres vus de dos. Cette notation utilise une portée horizontale de cinq lignes qui représente le corps."
        },
        {
          "@language": "de",
          "@value": "Eine Form der Bewegungsnotation, die abstrakte Symbole verwendet, um die Positionen des Körpers und der Gliedmaßen darzustellen, wie sie von hinten zu sehen sind. Sie wird auf fünfzeiligen horizontalen Notenlinien erfasst, die den Körper darstellen."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/noteMove"
      },
      "skos:prefLabel": [
        {
          "@language": "de",
          "@value": "Choreologie"
        },
        {
          "@language": "fr",
          "@value": "notation Benesh"
        },
        {
          "@language": "en",
          "@value": "Benesh movement notation"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/noteMove/1006",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "1032"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": [
        {
          "@language": "de",
          "@value": "Eine Form der Bewegungsnotation, die figurative und abstrakte Symbole verwendet, um die Positionen des Körpers und der Gliedmaßen und die Bewegungen darzustellen. Sie wird auf fünfzeiligen horizontalen Notenlinien erfasst, die den Körper darstellen."
        },
        {
          "@language": "fr",
          "@value": "Forme de notation du mouvement qui emploie des symboles figuratifs et abstraits pour représenter les positions et les mouvements du corps et des membres. Cette notation utilise une portée horizontale de cinq lignes qui représente le corps."
        },
        {
          "@language": "en",
          "@value": "A form of notated movement using figurative and abstract symbols to represent body and limb positions and movements. It is recorded on a five–line horizontal staff that represents the body."
        }
      ],
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/noteMove"
      },
      "skos:prefLabel": [
        {
