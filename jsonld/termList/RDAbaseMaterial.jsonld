  "@graph": [
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1012",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "449"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of calf, sheep, or goat skin which has been prepared to produce a thin, strong, translucent or opaque substance for writing, bookbinding, or other uses."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "parchment"
      },
      "skos:related": [
        {
          "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1017"
        },
        {
          "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1021"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1013",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "450"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of a powder prepared from calcium sulphate dihydrate (gypsum) or calcium carbonate and mixed with water and sometimes a filler to form a paste that liberates heat and then hardens."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "plaster"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1010",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "447"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of a substance typified by being a good conductor of electricity and heat, opaque with a characteristic lustre, fusible, and usually malleable or ductile."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "metal"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1011",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "448"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of thin material made from felted sheets or webs of animal, plant, mineral, or synthetic fibres formed and dried from a suspension in water."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "paper"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1016",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "453"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of lac, a resinous substance excreted by the female lac insect, that can be dissolved in ethyl alcohol to form a liquid that can be applied with a brush."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "shellac"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1017",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "454"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of the integument of animals, such as sheep, goats, or calves, separated from the body and variously processed to remove hair, dry, tan, or otherwise dress."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "skin"
      },
      "skos:related": [
        {
          "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1012"
        },
        {
          "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1009"
        }
      ]
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1014",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "451"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of synthetic or semi-synthetic organic polymers of high molecular weight that are moldable."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "plastic"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1015",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "452"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of a refractory white clay (kaolin) and a feldspathic rock that is heated to form a ceramic material."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "porcelain"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1018",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "455"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of rock in its naturally occurring shape or that has been cut, shaped, crushed, or otherwise formed."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "stone"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1019",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "456"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material created by processing man-made materials, usually as a substitute for a natural material."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "synthetic"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1024",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "461"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of the principal tissue of trees and similar plants."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "wood"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1023",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "460"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of a chemical compound from an animal, plant, mineral, or synthetic source that is malleable near ambient temperatures, slightly greasy to the touch, with a low melting point, and usually translucent, water-repellant, and soluble in organic solvents."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "wax"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1022",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "459"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of a polymer or copolymer derived from a vinyl (ethenyl) group, typically vinyl chloride."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "vinyl"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1021",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "458"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of fine-quality calf or lamb parchment."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "vellum"
      },
      "skos:related": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1012"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1020",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "457"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material produced by weaving, felting, knotting, twining, or otherwise processing natural or synthetic fibers so that they cohere."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "textile"
      },
      "skos:scopeNote": {
        "@language": "en",
        "@value": "Excludes fibreboard, paper, papier-mâché, and papyrus."
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1009",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "446"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of the skin or hide of an animal that has been tanned to render it resistant to putrefaction and relatively soft and flexible when dry."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "leather"
      },
      "skos:related": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1017"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1008",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "445"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of the dentine forming the bulk of the teeth and tusks of animals such as elephants, walruses, and narwhals."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "ivory"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1005",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "442"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of silicon dioxide (silica) fused with a basic oxide; generally transparent but often translucent or opaque."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "glass"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1004",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "441"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "Base material consisting of a nonmetallic mineral, such as clay, fired at a high temperature to form a hard, brittle, heat- and corrosion-resistant material."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "ceramic"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1007",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "444"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of laminated paper board that has paper layers glued to its surface; used commonly as temporary artists' supports."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "illustration board"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1006",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "443"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of any firm, dense, rigid board, often manufactured from fiber consolidated under heat and pressure in a hot press."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "hardboard"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1001",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "438"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of a high-grade white cardboard, supercalendered with China clay or made by pasting together sheets of heavy ledger paper."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "Bristol board"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1003",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "440"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of a type of stiff pasteboard that is thicker than 0.006 inches, typically consisting of good-quality chemical pulp or rag pasteboard, and varying greatly in type and stability."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "cardboard"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial/1002",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "439"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A base material consisting of a closely woven textile made in various weights, usually of flax, hemp, jute, or cotton, used as a support for painting or printing; or a loosely woven, lattice-like mesh, usually of flax, hemp, jute, or cotton, used as a needlepoint foundation."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAbaseMaterial"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "canvas"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAbaseMaterial",
      "@type": "skos:ConceptScheme",
      "dc:title": "RDA Base Material"
    },
