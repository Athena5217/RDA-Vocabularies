<!--Property: Current owner (Item)-->
<rdf:Description rdf:about="http://rdvocab.info/roles/currentOwnerItem">
  <rdfs:isDefinedBy rdf:resource="http://rdvocab.info/roles" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002" />
  <reg:name xml:lang="en">currentOwnerItem</reg:name>
  <rdfs:label xml:lang="en">Current owner (Item)</rdfs:label>
  <skos:definition xml:lang="en">A person, family, or corporate body currently having legal possession of an item (i.e., a specific copy or instance of a resource).</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:subPropertyOf rdf:resource="http://rdvocab.info/roles/ownerItem" />
  <rdfs:domain rdf:resource="http://rdvocab.info/uri/schema/FRBRentitiesRDA/Item" />
  <rdfs:label xml:lang="de">Derzeitiger Besitzer (Exemplar)</rdfs:label>
  <skos:definition xml:lang="de">Eine Person, Familie oder Körperschaft, die derzeit rechtmäßig im Besitz eines Exemplars ist (z. B. einer bestimmten Kopie oder eines bestimmten Exemplars einer Ressource).</skos:definition>
</rdf:Description>

