<!--Property: Production company (Work)-->
<rdf:Description rdf:about="http://rdvocab.info/roles/productionCompanyWork">
  <rdfs:isDefinedBy rdf:resource="http://rdvocab.info/roles" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002" />
  <reg:name xml:lang="en">productionCompanyWork</reg:name>
  <rdfs:label xml:lang="en">Production company (Work)</rdfs:label>
  <skos:definition xml:lang="en">A corporate body that is responsible for financial, technical, and organizational management of a production for stage, screen, sound recording, television, webcast, etc.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdvocab.info/uri/schema/FRBRentitiesRDA/Work" />
  <rdfs:subPropertyOf rdf:resource="http://rdvocab.info/roles/productionCompany" />
  <rdfs:label xml:lang="de">Produktionsunternehmen (Werk)</rdfs:label>
  <skos:definition xml:lang="de">Eine Körperschaft, die für das finanzielle, technische und organisatorische Management einer Produktion für Bühne, Leinwand, Tonaufnahme, Fernsehen, Internetübertragung o.ä. verantwortlich ist.</skos:definition>
</rdf:Description>

