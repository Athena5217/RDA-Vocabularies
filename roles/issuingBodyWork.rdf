<!--Property: Issuing body (Work)-->
<rdf:Description rdf:about="http://rdvocab.info/roles/issuingBodyWork">
  <rdfs:isDefinedBy rdf:resource="http://rdvocab.info/roles" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002" />
  <reg:name xml:lang="en">issuingBodyWork</reg:name>
  <rdfs:label xml:lang="en">Issuing body (Work)</rdfs:label>
  <skos:definition xml:lang="en">A person, family or corporate body issuing the work, such as an official organ of the body. </skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdvocab.info/uri/schema/FRBRentitiesRDA/Work" />
  <rdfs:subPropertyOf rdf:resource="http://rdvocab.info/roles/issuingBody" />
  <rdfs:label xml:lang="de">Veröffentlichendes Organ (Werk)</rdfs:label>
  <skos:definition xml:lang="de">Eine Person, Familie oder Körperschaft, die ein Werk herausgibt, wie z.B. ein offizielles Organ der Körperschaft. </skos:definition>
</rdf:Description>

