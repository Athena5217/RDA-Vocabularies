<!--Property: Performer (Expression)-->
<rdf:Description rdf:about="http://rdvocab.info/roles/performerExpression">
  <rdfs:isDefinedBy rdf:resource="http://rdvocab.info/roles" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002" />
  <reg:name xml:lang="en">performerExpression</reg:name>
  <rdfs:label xml:lang="en">Performer (Expression)</rdfs:label>
  <skos:definition xml:lang="en">A person, family, or corporate body contributing to an expression of a work by performing music, acting, dancing, speaking, etc., often in a musical or dramatic presentation, etc.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <reg:hasSubproperty rdf:resource="http://rdvocab.info/roles/actorExpression" />
  <reg:hasSubproperty rdf:resource="http://rdvocab.info/roles/dancerExpression" />
  <reg:hasSubproperty rdf:resource="http://rdvocab.info/roles/instrumentalistExpression" />
  <reg:hasSubproperty rdf:resource="http://rdvocab.info/roles/narratorExpression" />
  <reg:hasSubproperty rdf:resource="http://rdvocab.info/roles/singerExpression" />
  <reg:hasSubproperty rdf:resource="http://rdvocab.info/roles/storytellerExpression" />
  <rdfs:subPropertyOf rdf:resource="http://rdvocab.info/roles/contributor" />
  <rdfs:domain rdf:resource="http://rdvocab.info/uri/schema/FRBRentitiesRDA/Expression" />
  <reg:hasSubproperty rdf:resource="http://rdvocab.info/roles/speakerExpression" />
  <rdfs:label xml:lang="de">Darsteller (Expression)</rdfs:label>
  <skos:definition xml:lang="de">Eine Person, Familie oder Körperschaft, die für die Expression eines Werkes Musik aufführen, auftreten, tanzen, sprechen usw., oft in einer musikalischen oder dramatischen Darbietung usw.</skos:definition>
</rdf:Description>

