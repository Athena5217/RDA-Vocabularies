<!--Property: Collection registrar (Item)-->
<rdf:Description rdf:about="http://rdvocab.info/roles/collectionRegistrarItem">
  <rdfs:isDefinedBy rdf:resource="http://rdvocab.info/roles" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002" />
  <reg:name xml:lang="en">collectionRegistrarItem</reg:name>
  <rdfs:label xml:lang="en">Collection registrar (Item)</rdfs:label>
  <skos:definition xml:lang="en">A type of curator who lists or inventories the items in an aggregate work such as a collection of items or works.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdvocab.info/uri/schema/FRBRentitiesRDA/Item" />
  <rdfs:label xml:lang="de">Registrar (Exemplar)</rdfs:label>
  <skos:definition xml:lang="de">Jemand, der die Exemplare eines zusammengestellten Werkes, wie beispielsweise eine Sammlung von Exemplaren oder Werken, verzeichnet und inventarisiert.</skos:definition>
</rdf:Description>

