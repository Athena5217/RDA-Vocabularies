<!--Property: Depositor (Item)-->
<rdf:Description rdf:about="http://rdvocab.info/roles/depositorItem">
  <rdfs:isDefinedBy rdf:resource="http://rdvocab.info/roles" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002" />
  <reg:name xml:lang="en">depositorItem</reg:name>
  <rdfs:label xml:lang="en">Depositor (Item)</rdfs:label>
  <skos:definition xml:lang="en">A current owner of an item who deposited the item into the custody of another person, family, or corporate body, while still retaining ownership.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdvocab.info/uri/schema/FRBRentitiesRDA/Item" />
  <rdfs:label xml:lang="de">Leihgeber (Exemplar)</rdfs:label>
  <skos:definition xml:lang="de">Der gegenwärtige Eigentümer eines Exemplars, der das Exemplar in die Obhut einer anderen Person, Familie oder Körperschaft gegeben hat, während er weiterhin der Eigentümer bleibt.</skos:definition>
</rdf:Description>

