<!--Property: Recordist (Expression)-->
<rdf:Description rdf:about="http://rdvocab.info/roles/recordistExpression">
  <rdfs:isDefinedBy rdf:resource="http://rdvocab.info/roles" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002" />
  <reg:name xml:lang="en">recordistExpression</reg:name>
  <rdfs:label xml:lang="en">Recordist (Expression)</rdfs:label>
  <skos:definition xml:lang="en">A person, family, or corporate body contributing to an expression of a work by using a recording device to capture sound and/or video during a recording session, including field recordings of natural sounds, folkloric events, music, etc.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:subPropertyOf rdf:resource="http://rdvocab.info/roles/contributor" />
  <rdfs:domain rdf:resource="http://rdvocab.info/uri/schema/FRBRentitiesRDA/Expression" />
  <rdfs:label xml:lang="de">Recordist (Expression)</rdfs:label>
  <skos:definition xml:lang="de">Eine Person, Familie oder Körperschaft, die an einer Expression eines Werkes mitwirkt, indem sie mit einem Aufzeichnungsgerät Ton und/oder Bild aufnimmt, einschließlich Außenaufnahmen natürlicher Geräusche, folkloristischer Veranstaltungen, Musik o.ä.</skos:definition>
</rdf:Description>

