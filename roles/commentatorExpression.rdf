<!--Property: Commentator (Expression)-->
<rdf:Description rdf:about="http://rdvocab.info/roles/commentatorExpression">
  <rdfs:isDefinedBy rdf:resource="http://rdvocab.info/roles" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002" />
  <reg:name xml:lang="en">commentatorExpression</reg:name>
  <rdfs:label xml:lang="en">Commentator (Expression)</rdfs:label>
  <skos:definition xml:lang="en">A performer contributing to an expression of a work by providing interpretation, analysis, or a discussion of the subject matter on a recording, film, or other audiovisual medium.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdvocab.info/uri/schema/FRBRentitiesRDA/Expression" />
  <rdfs:label xml:lang="de">Kommentator (mündlich) (Expression)</rdfs:label>
  <skos:definition xml:lang="de">Jemand, der zu einer Expression eines Werkes in einer Aufnahme, in einem Film oder in einem anderen audiovisuellen Medium in Form einer Interpretation, einer Analyse oder einer Diskussion des Themas beiträgt.</skos:definition>
</rdf:Description>

