<http://metadataregistry.org/uri/RegStatus/1001>
    a :Concept ;
    :prefLabel "Published"@en .

<http://rdaregistry.info/termList/FormatNoteMus>
    dc:title "RDA Format of Notated Music" ;
    a :ConceptScheme .

<http://rdaregistry.info/termList/formatNoteMus/1001>
    reg:identifier <file:///var/www/registry/web/repos/RDA-Vocabularies/xml/termList/1153> ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    a :Concept ;
    :definition "A large music book made to be placed on a stand in front of a choir. Each part is notated separately, usually in the configuration that presents, when the book is open, the soprano and tenor parts on the verso of a leaf, and the alto and bass parts on the recto of the next leaf."@en ;
    :inScheme <http://rdaregistry.info/termList/FormatNoteMus> ;
    :prefLabel "choir book"@en .

<http://rdaregistry.info/termList/formatNoteMus/1002>
    reg:identifier <file:///var/www/registry/web/repos/RDA-Vocabularies/xml/termList/1154> ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    a :Concept ;
    :definition "A score of a work for solo voices and chorus showing only the parts for chorus, at least in those portions of the work in which the chorus sings, with the instrumental accompaniment either arranged for keyboard(s) or other chordal instruments(s) or omitted."@en ;
    :inScheme <http://rdaregistry.info/termList/FormatNoteMus> ;
    :prefLabel "chorus score"@en .

<http://rdaregistry.info/termList/formatNoteMus/1003>
    reg:identifier <file:///var/www/registry/web/repos/RDA-Vocabularies/xml/termList/1155> ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    a :Concept ;
    :altLabel "close score"@en, "reduced score"@en, "short score"@en ;
    :definition "A score in which the number of staves is reduced to two or a few, generally organized by instrumental sections or vocal parts, and often with cues for individual parts. Used for: Close score, Reduced score, Short score."@en ;
    :inScheme <http://rdaregistry.info/termList/FormatNoteMus> ;
    :prefLabel "condensed score"@en .

<http://rdaregistry.info/termList/formatNoteMus/1004>
    reg:identifier <file:///var/www/registry/web/repos/RDA-Vocabularies/xml/termList/1156> ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    a :Concept ;
    :definition "In the context of notated music, a component consisting of the music for the use of one or more, but not all, performers."@en ;
    :inScheme <http://rdaregistry.info/termList/FormatNoteMus> ;
    :prefLabel "part"@en .

<http://rdaregistry.info/termList/formatNoteMus/1005>
    reg:identifier <file:///var/www/registry/web/repos/RDA-Vocabularies/xml/termList/1157> ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    a :Concept ;
    :definition "A performance part for a piano performer in an ensemble, with cues for the other instruments that enable the performer of that part also to conduct."@en ;
    :inScheme <http://rdaregistry.info/termList/FormatNoteMus> ;
    :prefLabel "piano conductor part"@en .

<http://rdaregistry.info/termList/formatNoteMus/1006>
    reg:identifier <file:///var/www/registry/web/repos/RDA-Vocabularies/xml/termList/1158> ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    a :Concept ;
    :definition "A reduction of an instrumental work or a vocal work with instruments to a version for piano. May include the words of a vocal work."@en ;
    :inScheme <http://rdaregistry.info/termList/FormatNoteMus> ;
    :prefLabel "piano score"@en .

<http://rdaregistry.info/termList/formatNoteMus/1007>
    reg:identifier <file:///var/www/registry/web/repos/RDA-Vocabularies/xml/termList/1159> ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    a :Concept ;
    :altLabel "full score"@en ;
    :definition "Graphical, symbolic, or word-based musical notation representing the sounds of all the parts of an ensemble or a work for solo performer or electronic media. "@en ;
    :inScheme <http://rdaregistry.info/termList/FormatNoteMus> ;
    :prefLabel "score"@en ;
    :scopeNote "Do not confuse with Part."@en .

<http://rdaregistry.info/termList/formatNoteMus/1008>
    reg:identifier <file:///var/www/registry/web/repos/RDA-Vocabularies/xml/termList/1160> ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    a :Concept ;
    :definition "A score issued in a musical image of reduced size, not primarily intended for use in performance. A descriptive phrase such as \"Study score\", \"Miniature score\", \"Taschenpartitur\", \"Partition de poche\", etc., usually appears on the resource."@en ;
    :inScheme <http://rdaregistry.info/termList/FormatNoteMus> ;
    :prefLabel "study score"@en .

<http://rdaregistry.info/termList/formatNoteMus/1009>
    reg:identifier <file:///var/www/registry/web/repos/RDA-Vocabularies/xml/termList/1161> ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    a :Concept ;
    :definition "A music book made to be placed on a table and displayed in such a way that the performers can read their parts while seated or standing across or around the table. Each part is notated separately, usually in a configuration that presents, when the book is open, different parts in inverted and/or perpendicular positions."@en ;
    :inScheme <http://rdaregistry.info/termList/FormatNoteMus> ;
    :prefLabel "table book"@en .

<http://rdaregistry.info/termList/formatNoteMus/1010>
    reg:identifier <file:///var/www/registry/web/repos/RDA-Vocabularies/xml/termList/1162> ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    a :Concept ;
    :definition "A performance part for a violin performer in an ensemble, with cues for the other instruments that enable the performer of that part also to conduct."@en ;
    :inScheme <http://rdaregistry.info/termList/FormatNoteMus> ;
    :prefLabel "violin conductor part"@en .

<http://rdaregistry.info/termList/formatNoteMus/1011>
    reg:identifier <file:///var/www/registry/web/repos/RDA-Vocabularies/xml/termList/1163> ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    a :Concept ;
    :definition "A score showing all vocal parts, with the instrumental accompaniment either arranged for keyboard(s) or other chordal instrument(s) or omitted."@en ;
    :inScheme <http://rdaregistry.info/termList/FormatNoteMus> ;
    :prefLabel "vocal score"@en .

