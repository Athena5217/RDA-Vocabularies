<!-- Scheme: RDA Content Type -->
    <skos:ConceptScheme rdf:about="http://rdvocab.info/termList/RDAContentType">
        <dc:title>RDA Content Type</dc:title>
    </skos:ConceptScheme>

<!-- Concept: cartographic dataset  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAContentType/1001" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAContentType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">cartographic dataset</skos:prefLabel>
        <skos:definition xml:lang="en">Cartographic content expressed through a digitally encoded dataset intended to be processed by a computer. </skos:definition>
        <skos:prefLabel xml:lang="de">kartografische Datei</skos:prefLabel>
        <skos:definition xml:lang="de">Kartografischer Inhalt, der über eine digital codierte Datei dargestellt wird und für die Verarbeitung durch einen Computer konzipiert wurde.</skos:definition>
    </skos:Concept>
<!-- Concept: cartographic image  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAContentType/1002" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAContentType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">cartographic image</skos:prefLabel>
        <skos:definition xml:lang="en">Cartographic content expressed through line, shape, shading, etc., intended to be perceived visually as a still image or images in two dimensions. Includes maps, views, atlases, remote-sensing images, etc.</skos:definition>
        <skos:prefLabel xml:lang="de">kartografisches Bild</skos:prefLabel>
        <skos:definition xml:lang="de">Kartografischer Inhalt, der durch Linie, Form, Schattierung usw. dargestellt wird und konzipiert wurde, um visuell als einzelnes Bild oder als zweidimensionale Bilder wahrgenommen zu werden. Dazu gehören Karten, Ansichten, Atlanten, Fernerkundungsbilder usw.</skos:definition>
    </skos:Concept>
<!-- Concept: cartographic moving image  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAContentType/1003" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAContentType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">cartographic moving image</skos:prefLabel>
        <skos:definition xml:lang="en">Cartographic content expressed through images intended to be perceived as moving, in two dimensions. Includes satellite images of the Earth or other celestial bodies in motion.</skos:definition>
        <skos:prefLabel xml:lang="de">kartografisches bewegtes Bild</skos:prefLabel>
        <skos:definition xml:lang="de">Kartografischer Inhalt, der durch Bilder dargestellt wird, die konzipiert wurden, um als zweidimensionale Bewegung wahrgenommen zu werden. Dazu gehören Satellitenbilder der Erde oder anderer Himmelskörper, die in Bewegung sind.</skos:definition>
    </skos:Concept>
<!-- Concept: cartographic tactile image  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAContentType/1004" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAContentType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">cartographic tactile image</skos:prefLabel>
        <skos:definition xml:lang="en">Cartographic content expressed through line, shape, and/or other forms, intended to be perceived through touch as a still image in two dimensions.</skos:definition>
        <skos:prefLabel xml:lang="de">kartografisches taktiles Bild</skos:prefLabel>
        <skos:definition xml:lang="de">Kartografischer Inhalt, der durch Linie, Form und/oder andere Gestaltungen dargestellt wird und konzipiert wurde, um über den Tastsinn als ein zweidimensionales Bild wahrgenommen zu werden.</skos:definition>
    </skos:Concept>
<!-- Concept: cartographic tactile three-dimensional form  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAContentType/1005" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAContentType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">cartographic tactile three-dimensional form</skos:prefLabel>
        <skos:definition xml:lang="en">Cartographic content expressed through the form intended to be perceived through touch as a three-dimensional form or forms.</skos:definition>
        <skos:prefLabel xml:lang="de">kartografische taktile dreidimensionale Form</skos:prefLabel>
        <skos:definition xml:lang="de">Kartografischer Inhalt, der durch eine Form oder mehrere Formen dargestellt wird und konzipiert wurde, um über den Tastsinn als dreidimensionale Form(en) wahrgenommen zu werden.</skos:definition>
    </skos:Concept>
<!-- Concept: cartographic three-dimensional form  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAContentType/1006" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAContentType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">cartographic three-dimensional form</skos:prefLabel>
        <skos:definition xml:lang="en">Cartographic content expressed through a form or forms intended to be perceived visually in three-dimensions. Includes globes, relief models, etc.</skos:definition>
        <skos:prefLabel xml:lang="de">kartografische dreidimensionale Form </skos:prefLabel>
        <skos:definition xml:lang="de">Kartografischer Inhalt, der durch eine Form bzw. Formen dargestellt wird und konzipiert wurde, um visuell dreidimensional wahrgenommen zu werden. Dazu gehören Globen, Reliefmodelle usw.</skos:definition>
    </skos:Concept>
<!-- Concept: computer dataset  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAContentType/1007" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAContentType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">computer dataset</skos:prefLabel>
        <skos:definition xml:lang="en">Content expressed through a digitally encoded dataset intended to be processed by a computer. Includes numeric data, environmental data, etc., used by applications software to calculate averages, correlations, etc., or to produce models, etc., but not normally displayed in its raw form. </skos:definition>
        <skos:prefLabel xml:lang="de">Computerdatei</skos:prefLabel>
        <skos:scopeNote xml:lang="de">Kartografische Daten siehe kartografische Datei.</skos:scopeNote>
        <skos:definition xml:lang="de">Inhalt, der über eine digital codierte Datei dargestellt wird und für die Verarbeitung durch einen Computer konzipiert wurde. Dazu gehören Daten wie numerische Daten, Umweltdaten usw., die von Anwendersoftware verwendet werden, um Durchschnittsmengen, Korrelationen usw. oder um Modelle usw. zu erstellen. Die Daten werden normalerweise nicht in Form von Rohdaten präsentiert.</skos:definition>
    </skos:Concept>
<!-- Concept: computer program  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAContentType/1008" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAContentType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">computer program</skos:prefLabel>
        <skos:definition xml:lang="en">Content expressed through digitally encoded instructions intended to be processed and performed by a computer. Includes operating systems, applications software, etc.</skos:definition>
        <skos:prefLabel xml:lang="de">Computerprogramm</skos:prefLabel>
        <skos:definition xml:lang="de">Inhalt, der durch digital codierte Anweisungen ausgedrückt wird und für Verarbeitung und Ausführung mit Hilfe eines Computers konzipiert wurde. Dazu gehören Betriebssysteme, Anwendersoftware usw.</skos:definition>
    </skos:Concept>
<!-- Concept: notated movement  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAContentType/1009" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAContentType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">notated movement</skos:prefLabel>
        <skos:definition xml:lang="en">Content expressed through a form of notation intended to be perceived visually. </skos:definition>
        <skos:scopeNote xml:lang="en">Includes all forms of movement notation other than those intended to be perceived through touch.</skos:scopeNote>
        <skos:prefLabel xml:lang="de">notierte Bewegung</skos:prefLabel>
        <skos:scopeNote xml:lang="de">Dazu gehören alle Formen der Bewegungsnotation außer denjenigen, die bestimmt sind, über den Tastsinn wahrgenommen zu werden.</skos:scopeNote>
        <skos:definition xml:lang="de">Inhalt, der durch ein Notationssystem für Bewegung dargestellt wird und konzipiert wurde, um visuell wahrgenommen zu werden.</skos:definition>
    </skos:Concept>
<!-- Concept: notated music  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAContentType/1010" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAContentType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">notated music</skos:prefLabel>
        <skos:definition xml:lang="en">Content expressed through a form of musical notation intended to be perceived visually. </skos:definition>
        <skos:scopeNote xml:lang="en">Includes all forms of music notation other than those intended to be perceived through touch.</skos:scopeNote>
        <skos:prefLabel xml:lang="de">schriftlich fixierte Musik</skos:prefLabel>
        <skos:scopeNote xml:lang="de">Dazu gehören alle Formen der notierten Musik, außer derjenigen, die konzipiert wurde, um über den Tastsinn wahrgenommen zu werden.</skos:scopeNote>
        <skos:definition xml:lang="de">Inhalt, der durch ein Notationssystem für Musik ausgedrückt wird und konzipiert wurde, um visuell wahrgenommen zu werden.</skos:definition>
    </skos:Concept>
<!-- Concept: performed music  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAContentType/1011" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAContentType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">performed music</skos:prefLabel>
        <skos:definition xml:lang="en">Content expressed through music in an audible form.</skos:definition>
        <skos:scopeNote xml:lang="en">Includes recorded performances of music, computer-generated music, etc.</skos:scopeNote>
        <skos:prefLabel xml:lang="de">aufgeführte Musik</skos:prefLabel>
        <skos:scopeNote xml:lang="de">Dazu gehören aufgezeichnete Musikaufführungen, maschinell erzeugte Musik usw.</skos:scopeNote>
        <skos:definition xml:lang="de">Inhalt, der durch Musik in hörbarer Form ausgedrückt wird.</skos:definition>
    </skos:Concept>
<!-- Concept: sounds  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAContentType/1012" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAContentType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">sounds</skos:prefLabel>
        <skos:definition xml:lang="en">Content other than language or music, expressed in an audible form. </skos:definition>
        <skos:scopeNote xml:lang="en">Includes natural sounds, artificially produced sounds, etc.</skos:scopeNote>
        <skos:prefLabel xml:lang="de">Schallereignis</skos:prefLabel>
        <skos:scopeNote xml:lang="de">Anderer Inhalt als Sprache oder Musik, ausgedrückt in einer hörbaren Form.</skos:scopeNote>
        <skos:definition xml:lang="de">Dazu gehören natürliche Schallereignisse, künstlich erzeugte Schallereignisse usw.</skos:definition>
    </skos:Concept>
<!-- Concept: spoken word  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAContentType/1013" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAContentType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">spoken word</skos:prefLabel>
        <skos:definition xml:lang="en">Content expressed through language in an audible form.</skos:definition>
        <skos:scopeNote xml:lang="en">Includes recorded readings, recitations, speeches, interviews, oral histories, etc., computer-generated speech, etc.</skos:scopeNote>
        <skos:prefLabel xml:lang="de">gesprochenes Wort</skos:prefLabel>
        <skos:scopeNote xml:lang="de">Dazu gehören aufgezeichnete Lesungen, Rezitationen, Reden, Interviews, mündliche Überlieferungen usw. und maschinell erzeugte Sprache.</skos:scopeNote>
        <skos:definition xml:lang="de">Inhalt, der durch Sprache in einer hörbaren Form ausgedrückt wird.</skos:definition>
    </skos:Concept>
<!-- Concept: still image  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAContentType/1014" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAContentType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">still image</skos:prefLabel>
        <skos:definition xml:lang="en">Content expressed through line, shape, shading, etc., intended to be perceived visually as a still image or images in two dimensions. </skos:definition>
        <skos:scopeNote xml:lang="en">Includes drawings, paintings, diagrams, photographic images (stills), etc. </skos:scopeNote>
        <skos:prefLabel xml:lang="de">einzelnes Bild</skos:prefLabel>
        <skos:scopeNote xml:lang="de">Dazu gehören Zeichnungen, Gemälde, Diagramme, Standfotos, Einzelaufnahmen usw.</skos:scopeNote>
        <skos:definition xml:lang="de">Inhalt, der durch Linie, Form, Schattierung usw. ausgedrückt wird und konzipiert wurde, um visuell als einzelnes Bild oder Bilder zweidimensional wahrgenommen zu werden.</skos:definition>
    </skos:Concept>
<!-- Concept: tactile image  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAContentType/1015" xml:lang="en">
