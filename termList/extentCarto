<!-- Concept: Globe  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/extentCarto/1003" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/extentCarto"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Globe</skos:prefLabel>
        <skos:definition xml:lang="en">A depiction of the Earth or other celestial body (real or imaginary) on the surface of a sphere.</skos:definition>
    </skos:Concept>
<!-- Concept: Globes  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/extentCarto/1012" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/extentCarto"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Globes</skos:prefLabel>
        <skos:definition xml:lang="en">Depictions of the Earth or other celestial body (real or imaginary) on the surface of a sphere.</skos:definition>
    </skos:Concept>
<!-- Concept: Map  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/extentCarto/1004" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/extentCarto"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Map</skos:prefLabel>
        <skos:definition xml:lang="en">A representation, normally to scale and on a two-dimensional medium, of a selection of material or abstract features on, or in relation to, the surface of Earth, another celestial body, or an imaginary place.</skos:definition>
    </skos:Concept>
<!-- Concept: Maps  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/extentCarto/1013" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/extentCarto"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Maps</skos:prefLabel>
        <skos:definition xml:lang="en">Representations, normally to scale and on a two-dimensional medium, of a selection of material or abstract features on, or in relation to, the surface of Earth, another celestial body, or an imaginary place</skos:definition>
    </skos:Concept>
<!-- Concept: Model  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/extentCarto/1005" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/extentCarto"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Model</skos:prefLabel>
        <skos:definition xml:lang="en">1. A three-dimensional representation of a real or imagined object. 2. A three-dimensional representation of the whole or part of the Earth or any celestial body (real or imaginary) at any scale.</skos:definition>
    </skos:Concept>
<!-- Concept: Models  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/extentCarto/1014" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/extentCarto"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Models</skos:prefLabel>
        <skos:definition xml:lang="en">1. Three-dimensional representations of a real or imagined object. 2. Three-dimensional representations of the whole or part of the Earth or any celestial body (real or imaginary) at any scale.</skos:definition>
    </skos:Concept>
<!-- Concept: Profile  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/extentCarto/1006" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/extentCarto"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Profile</skos:prefLabel>
        <skos:definition xml:lang="en">In cartography, a scale representation of the intersection of a vertical surface (which may or may not be a plane) with the surface of the ground, or of the intersection of such a vertical surface with that of a conceptual three-dimensional model representing phenomena having a continuous distribution (e.g., rainfall).</skos:definition>
    </skos:Concept>
<!-- Concept: Profiles  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/extentCarto/1015" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/extentCarto"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Profiles</skos:prefLabel>
        <skos:definition xml:lang="en">In cartography, scale representations of the intersection of a vertical surface (which may or may not be a plane) with the surface of the ground, or of the intersection of such a vertical surface with that of a conceptual three-dimensional model representing phenomena having a continuous distribution (e.g., rainfall).</skos:definition>
    </skos:Concept>
