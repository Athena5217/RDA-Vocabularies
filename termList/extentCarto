<!-- Concept: Atlas  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/extentCarto/1001" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/extentCarto"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Atlas</skos:prefLabel>
        <skos:definition xml:lang="en">A volume of maps or other cartographic content, with or without descriptive text.</skos:definition>
    </skos:Concept>
