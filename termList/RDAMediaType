    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:reg="http://metadataregistry.org/uri/schema/registry/">

<!-- Scheme: RDA Media Type -->
    <skos:ConceptScheme rdf:about="http://rdvocab.info/termList/RDAMediaType">
        <dc:title>RDA Media Type</dc:title>
    </skos:ConceptScheme>

<!-- Concept: Audio  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAMediaType/1001" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAMediaType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Audio</skos:prefLabel>
        <skos:definition xml:lang="en">Media used to store recorded sound, designed for use with a playback device such as a turntable, audiocassette player, CD player, or MP3 player. </skos:definition>
        <skos:scopeNote xml:lang="en">Includes media used to store digitally encoded as well as analog sound.</skos:scopeNote>
        <skos:prefLabel xml:lang="de">auditive Medien</skos:prefLabel>
        <skos:definition xml:lang="de">Medien, die zur Speicherung von aufgenommenen Ton verwendet werden, und die mit Wiedergabegeräten wie Plattenspieler, Audiokassetten-Abspielgerät, CD-Player oder MP3-Player abgespielt werden.</skos:definition>
        <skos:scopeNote xml:lang="de">Dazu gehören sowohl Medien für die Speicherung von digital kodiertem als auch von analogem Ton.</skos:scopeNote>
    </skos:Concept>
<!-- Concept: Computer  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAMediaType/1003" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAMediaType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Computer</skos:prefLabel>
        <skos:definition xml:lang="en">Media used to store electronic files, designed for use with a computer. </skos:definition>
        <skos:scopeNote xml:lang="en">Includes media that are accessed remotely through file servers as well as direct-access media such as computer tapes and discs.</skos:scopeNote>
        <skos:prefLabel xml:lang="de">Computermedien</skos:prefLabel>
        <skos:scopeNote xml:lang="de">Dazu gehören sowohl Medien, die per Fernzugriff über Dateiserver aufgerufen werden als auch Medien im Direktzugriff, wie Computerbänder, Disketten, CD-ROMs und Festplatten.</skos:scopeNote>
        <skos:definition xml:lang="de">Medien, die zur Speicherung von elektronischen Dateien verwendet werden, die für die Computeranwendung konzipiert wurden.</skos:definition>
    </skos:Concept>
<!-- Concept: Microform  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAMediaType/1002" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAMediaType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Microform</skos:prefLabel>
        <skos:definition xml:lang="en">Media used to store reduced-size images not readable to the human eye, designed for use with a device such as a microfilm or microfiche reader.</skos:definition>
        <skos:scopeNote xml:lang="en">Includes both transparent and opaque micrographic media.</skos:scopeNote>
        <skos:prefLabel xml:lang="de">Mikroformen</skos:prefLabel>
        <skos:scopeNote xml:lang="de">Dazu gehören sowohl transparente als auch llichtundurchlässige, mikrografische Medien.</skos:scopeNote>
        <skos:definition xml:lang="de">Medien, die zur Speicherung von verkleinerten Bildern verwendet werden, die für das menschliche Auge nicht lesbar sind und für die Benutzung mithilfe von Geräten wie Mikrofilm- oder Mikrofiche-Lesegeräten konzipiert wurden.</skos:definition>
    </skos:Concept>
<!-- Concept: Microscopic  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAMediaType/1004" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAMediaType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Microscopic</skos:prefLabel>
        <skos:definition xml:lang="en">Media used to store minute objects, designed for use with a device such as a microscope to reveal details invisible to the naked eye.</skos:definition>
        <skos:prefLabel xml:lang="de">mikroskopische Medien</skos:prefLabel>
        <skos:definition xml:lang="de">Medien, die zur Speicherung winziger Objekte verwendet werden, die für die Benutzung mithilfe eines Geräts wie ein Mikroskop konzipiert wurden, das Details erkennen lässt, die für das bloße Auge nicht sichtbar sind. </skos:definition>
    </skos:Concept>
<!-- Concept: Projected  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAMediaType/1005" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAMediaType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Projected</skos:prefLabel>
        <skos:definition xml:lang="en">Media used to store moving or still images, designed for use with a projection device such as a motion picture film projector, slide projector, or overhead projector.  </skos:definition>
        <skos:scopeNote xml:lang="en">Includes media designed to project both two-dimensional and three-dimensional images.</skos:scopeNote>
        <skos:prefLabel xml:lang="de">projizierbare Medien</skos:prefLabel>
        <skos:scopeNote xml:lang="de">Dazu gehören sowohl Medien für die Projektion zweidimensionaler als auch dreidimensionaler Bilder.</skos:scopeNote>
        <skos:definition xml:lang="de">Medien, die für die Speicherung von bewegten oder stehenden Bildern verwendet werden, und die für die Benutzung mit einem Projektionsgerät wie einem Film-, Dia- oder Overhead-Projektor konzipiert wurden.</skos:definition>
    </skos:Concept>
<!-- Concept: Stereoscopic  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAMediaType/1006" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAMediaType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Stereoscopic</skos:prefLabel>
        <skos:definition xml:lang="en">Media used to store pairs of still images, designed for use with a device such as a stereoscope or stereograph viewer to give the effect of three dimensions.</skos:definition>
        <skos:prefLabel xml:lang="de">stereoskopische Medien</skos:prefLabel>
        <skos:definition xml:lang="de">Medien, die für die Speicherung von Stereo-Bildpaaren verwendet werden, die für die Betrachtung mit einem Gerät wie einem Stereoskop oder einem stereografischen Betrachter konzipiert wurden, um  den Eindruck von Dreidimensionalität zu erzeugen. </skos:definition>
    </skos:Concept>
<!-- Concept: Unmediated  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAMediaType/1007" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAMediaType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Unmediated</skos:prefLabel>
        <skos:definition xml:lang="en">Media used to store content designed to be perceived directly through one or more of the human senses without the aid of an intermediating device. </skos:definition>
        <skos:scopeNote xml:lang="en">Includes media containing visual and/or tactile content produced using processes such as printing, engraving, lithography, etc., embossing, texturing, etc., or by means of handwriting, drawing, painting, etc.  </skos:scopeNote>
        <skos:scopeNote xml:lang="en">Also includes media used to convey three-dimensional forms such as sculptures, models, etc.</skos:scopeNote>
        <skos:prefLabel xml:lang="de">ohne Hilfsmittel zu benutzende Medien</skos:prefLabel>
        <skos:scopeNote xml:lang="de">Dazu gehören Medien mit visuellem und/oder taktilem Inhalt, die mit Verfahren wie Druck, Gravur, Lithografie usw. oder Prägung, Texturierung usw. oder durch Handschrift, Zeichnen, Malen usw. hergestellt werden.</skos:scopeNote>
        <skos:scopeNote xml:lang="de">Dazu gehören auch Medien, die zur Vermittlung dreidimensionaler Formen verwendet werden wie Skulpturen, Modelle usw.</skos:scopeNote>
        <skos:definition xml:lang="de">Medien, die für die Speicherung von Inhalten verwendet werden, die ohne Hilfsmittel direkt durch einen oder mehreren der menschlichen Sinne wahrgenommen werden können.</skos:definition>
    </skos:Concept>
<!-- Concept: Video  -->
    <skos:Concept rdf:about="http://rdvocab.info/termList/RDAMediaType/1008" xml:lang="en">
        <skos:inScheme rdf:resource="http://rdvocab.info/termList/RDAMediaType"/>
        <skos:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002"/>
        <skos:prefLabel xml:lang="en">Video</skos:prefLabel>
        <skos:definition xml:lang="en">Media used to store moving or still images, designed for use with a playback device such as a videocassette player or DVD player. </skos:definition>
        <skos:scopeNote xml:lang="en">Includes media used to store digitally encoded as well as analog images.</skos:scopeNote>
        <skos:prefLabel xml:lang="de">Videomaterial</skos:prefLabel>
        <skos:scopeNote xml:lang="de">Dazu gehören sowohl Medien für die Speicherung von digital kodierten als auch von analogen Bildern. </skos:scopeNote>
        <skos:definition xml:lang="de">Medien, die für die Speicherung von bewegten oder einzelnen Bildern verwendet werden, und die für die Benutzung mit einem Wiedergabegerät wie einem Videokassetten- oder DVD-Player konzipiert wurden.</skos:definition>
    </skos:Concept>

<!-- Status properties used in this document  -->
    <skos:Concept rdf:about="http://metadataregistry.org/uri/RegStatus/1002">
        <skos:prefLabel xml:lang="en">New-Proposed</skos:prefLabel>
    </skos:Concept>

</rdf:RDF>