<div xmlns="http://www.w3.org/1999/xhtml"
  prefix="
    rdf: http://www.w3.org/1999/02/22-rdf-syntax-ns#
    skos: http://www.w3.org/2004/02/skos/core#
    reg: http://metadataregistry.org/uri/schema/registry/
    dc: http://purl.org/dc/elements/1.1/
    rdfs: http://www.w3.org/2000/01/rdf-schema#"
  >
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/RDAPolarity/1003">
    <div rel="skos:inScheme">
      <div typeof="skos:ConceptScheme" about="http://rdaregistry.info/termList/RDAPolarity">
        <div property="dc:title" content="RDA Polarity"></div>
      </div>
    </div>
    <div property="skos:definition" xml:lang="en" content="A polarity in which colours and tones used for images are a mixture of those seen with the human eye and those opposite what the human eye would see."></div>
    <div rel="reg:identifier" resource="http://rdf-translator.appspot.com/485"></div>
    <div property="skos:prefLabel" xml:lang="en" content="mixed polarity"></div>
    <div rel="reg:status">
      <div typeof="skos:Concept" about="http://metadataregistry.org/uri/RegStatus/1001">
        <div property="skos:prefLabel" xml:lang="en" content="Published"></div>
      </div>
    </div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/RDAPolarity/1001">
    <div rel="reg:identifier" resource="http://rdf-translator.appspot.com/483"></div>
    <div property="skos:definition" xml:lang="en" content="A polarity in which colours and tones used for images are the same as those seen with the human eye."></div>
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/RDAPolarity"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div property="skos:prefLabel" xml:lang="en" content="positive"></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/RDAPolarity/1002">
    <div property="skos:definition" xml:lang="en" content="A polarity in which colours and tones used for images are opposite those seen with the human eye."></div>
    <div rel="reg:identifier" resource="http://rdf-translator.appspot.com/484"></div>
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/RDAPolarity"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div property="skos:prefLabel" xml:lang="en" content="negative"></div>
  </div>
</div>