<div xmlns="http://www.w3.org/1999/xhtml"
  prefix="
    rdf: http://www.w3.org/1999/02/22-rdf-syntax-ns#
    skos: http://www.w3.org/2004/02/skos/core#
    reg: http://metadataregistry.org/uri/schema/registry/
    dc: http://purl.org/dc/elements/1.1/
    rdfs: http://www.w3.org/2000/01/rdf-schema#"
  >
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/AspectRatio/1001">
    <div rel="skos:inScheme">
      <div typeof="skos:ConceptScheme" about="http://rdaregistry.info/termList/AspectRatio">
        <div property="dc:title" content="RDA Aspect Ratio"></div>
      </div>
    </div>
    <div property="skos:definition" xml:lang="en" content="Aspect ratio for a moving image resource of less than 1.5:1."></div>
    <div property="skos:prefLabel" xml:lang="de" content="Vollbild"></div>
    <div property="skos:prefLabel" xml:lang="fr" content="plein écran"></div>
    <div rel="reg:identifier" resource="http://rdf-translator.appspot.com/727"></div>
    <div property="skos:prefLabel" xml:lang="es" content="pantalla completa"></div>
    <div property="skos:definition" xml:lang="es" content="Proporción dimensional de un recurso de imagen en movimiento menor que 1.5:1."></div>
    <div property="skos:prefLabel" xml:lang="en" content="full screen"></div>
    <div property="skos:definition" xml:lang="fr" content="Format de l’image d’une ressource d’images animées inférieur à 1,5:1."></div>
    <div property="skos:definition" xml:lang="de" content="Bildformat einer Bewegtbildressource von weniger als 1.5:1."></div>
    <div rel="reg:status">
      <div typeof="skos:Concept" about="http://metadataregistry.org/uri/RegStatus/1001">
        <div property="skos:prefLabel" xml:lang="en" content="Published"></div>
      </div>
    </div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/AspectRatio/1002">
    <div property="skos:definition" xml:lang="fr" content="Format de l’image d’une ressource d’images animées égal ou supérieur à 1,5:1."></div>
    <div property="skos:definition" xml:lang="en" content="Aspect ratio for a moving image resource of 1.5:1 or greater."></div>
    <div property="skos:prefLabel" xml:lang="en" content="wide screen"></div>
    <div property="skos:prefLabel" xml:lang="es" content="pantalla ancha"></div>
    <div rel="reg:identifier" resource="http://rdf-translator.appspot.com/728"></div>
    <div property="skos:prefLabel" xml:lang="fr" content="écran large"></div>
    <div property="skos:definition" xml:lang="de" content="Bildformat einer Bewegtbildressource von 1.5:1 oder höher."></div>
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/AspectRatio"></div>
    <div property="skos:prefLabel" xml:lang="de" content="Breitbild"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/AspectRatio/1003">
    <div property="skos:prefLabel" xml:lang="de" content="gemischt"></div>
    <div property="skos:prefLabel" xml:lang="fr" content="mixte"></div>
    <div property="skos:definition" xml:lang="en" content="Aspect ratio for a moving image resource that includes multiple aspect ratios within the
same resource."></div>
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/AspectRatio"></div>
    <div rel="reg:identifier" resource="http://rdf-translator.appspot.com/729"></div>
    <div property="skos:definition" xml:lang="de" content="Bildformat einer Bewegtbildressource, die mehrere Bildformate innerhalb derselben Ressource enthält."></div>
    <div property="skos:prefLabel" xml:lang="en" content="mixed"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div property="skos:prefLabel" xml:lang="es" content="mixta"></div>
    <div property="skos:definition" xml:lang="fr" content="Format de l’image d’une ressource d’images animées comprenant divers formats de l’image à l’intérieur de la même ressource."></div>
