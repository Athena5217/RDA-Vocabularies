<div xmlns="http://www.w3.org/1999/xhtml"
  prefix="
    rdf: http://www.w3.org/1999/02/22-rdf-syntax-ns#
    skos: http://www.w3.org/2004/02/skos/core#
    reg: http://metadataregistry.org/uri/schema/registry/
    dc: http://purl.org/dc/elements/1.1/
    rdfs: http://www.w3.org/2000/01/rdf-schema#"
  >
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/recMedium/1003">
    <div property="skos:prefLabel" xml:lang="en" content="optical"></div>
    <div rel="skos:inScheme">
      <div typeof="skos:ConceptScheme" about="http://rdaregistry.info/termList/recMedium">
        <div property="dc:title" content="RDA Recording Medium"></div>
      </div>
    </div>
    <div rel="reg:status">
      <div typeof="skos:Concept" about="http://metadataregistry.org/uri/RegStatus/1001">
        <div property="skos:prefLabel" xml:lang="en" content="Published"></div>
      </div>
    </div>
    <div property="skos:definition" xml:lang="en" content="Recording medium for recording binary encoded data in a transparent medium with a reflective backing. A laser is used to read the changes in reflectivity as a binary data stream."></div>
    <div rel="reg:identifier" resource="http://rdf-translator.appspot.com/865"></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/recMedium/1002">
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/recMedium"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div rel="reg:identifier" resource="http://rdf-translator.appspot.com/864"></div>
    <div property="skos:prefLabel" xml:lang="en" content="magneto-optical"></div>
    <div property="skos:definition" xml:lang="en" content="Recording medium that uses a combination of laser heating with a varying magnetic field to write data to a disc. The Sony MiniDisc is the most prominent format."></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/recMedium/1001">
    <div rel="reg:identifier" resource="http://rdf-translator.appspot.com/863"></div>
    <div property="skos:definition" xml:lang="en" content="Recording medium that relies on pole reversal in a ferromagnetic medium, typically ferromagnetic oxide particles bound to a continuous flexible surface (tape) or a rigid platter (disc)."></div>
    <div property="skos:prefLabel" xml:lang="en" content="magnetic"></div>
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/recMedium"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
  </div>
</div>