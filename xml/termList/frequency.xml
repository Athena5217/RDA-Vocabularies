      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
			<reg:identifier rdf:resource="758"/>
      <skos:prefLabel xml:lang="en">biennial</skos:prefLabel>
      <skos:definition xml:lang="en">Frequency for a resource issued or updated once every two years.</skos:definition>
      <skos:prefLabel xml:lang="de">alle zwei Jahre</skos:prefLabel>
      <skos:prefLabel xml:lang="fr">biennal</skos:prefLabel>
      <skos:definition xml:lang="de">Erscheinungsfrequenz für eine Ressource, die alle zwei Jahre erscheint oder aktualisiert wird.</skos:definition>
      <skos:definition xml:lang="fr">Périodicité d’une ressource publiée ou mise à jour tous les deux ans.</skos:definition>
      <skos:definition xml:lang="es">Frecuencia de un recurso publicado o actualizado una vez cada dos años.</skos:definition>
      <skos:prefLabel xml:lang="es">bienal</skos:prefLabel>
    </skos:Concept>
