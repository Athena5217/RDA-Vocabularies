      <skos:altLabel xml:lang="fr">Tous les quinze jours</skos:altLabel>
      <skos:prefLabel xml:lang="es">bisemanal</skos:prefLabel>
    </skos:Concept>
<!-- Concept: daily  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/frequency/1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/frequency"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
			<reg:identifier rdf:resource="745"/>
      <skos:prefLabel xml:lang="en">daily</skos:prefLabel>
      <skos:definition xml:lang="en">Frequency for a resource issued or updated once every day, usually exclusive of nonworking days.</skos:definition>
      <skos:prefLabel xml:lang="de">täglich</skos:prefLabel>
      <skos:prefLabel xml:lang="fr">quotidien</skos:prefLabel>
      <skos:definition xml:lang="de">Erscheinungsfrequenz für eine Ressource, die jeden Tag erscheint oder aktualisiert wird, normalerweise nicht an Tagen, die keine Arbeitstage sind.</skos:definition>
      <skos:definition xml:lang="fr">Périodicité d’une ressource publiée ou mise à jour une fois par jour, habituellement à l’exclusion des jours non ouvrables.</skos:definition>
      <skos:prefLabel xml:lang="es">diario</skos:prefLabel>
      <skos:definition xml:lang="es">Frecuencia de un recurso publicado o actualizado una vez cada día, generalmente exclusivo de días no laborables.</skos:definition>
    </skos:Concept>
<!-- Concept: irregular  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/frequency/1016" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/frequency"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
			<reg:identifier rdf:resource="760"/>
      <skos:prefLabel xml:lang="en">irregular</skos:prefLabel>
      <skos:definition xml:lang="en">Frequency for a resource issued with no consistent interval between issues.</skos:definition>
      <skos:prefLabel xml:lang="de">unregelmäßig</skos:prefLabel>
      <skos:prefLabel xml:lang="fr">irrégulier</skos:prefLabel>
      <skos:definition xml:lang="de">Erscheinungsfrequenz für eine Ressource, die ohne einheitliches Intervall zwischen den Ausgaben erscheint oder aktualisiert wird.</skos:definition>
      <skos:definition xml:lang="fr">Périodicité d’une ressource publiée à intervalles irréguliers.</skos:definition>
      <skos:definition xml:lang="es">Frecuencia de un recurso publicado sin intervalos consistentes entre entregas.</skos:definition>
      <skos:prefLabel xml:lang="es">irregular</skos:prefLabel>
    </skos:Concept>
