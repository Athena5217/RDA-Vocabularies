        <dc:title>RDA Frequency</dc:title>
    </skos:ConceptScheme>

<!-- Concept: annual  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/frequency/1013" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/frequency"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
			<reg:identifier rdf:resource="757"/>
      <skos:prefLabel xml:lang="en">annual</skos:prefLabel>
      <skos:definition xml:lang="en">Frequency for a resource issued or updated once every year.</skos:definition>
      <skos:prefLabel xml:lang="de">jährlich</skos:prefLabel>
      <skos:prefLabel xml:lang="fr">annuel</skos:prefLabel>
      <skos:definition xml:lang="de">Erscheinungsfrequenz für eine Ressource, die einmal pro Jahr erscheint oder aktualisiert wird.</skos:definition>
      <skos:definition xml:lang="fr">Périodicité d’une ressource publiée ou mise à jour une fois par année.</skos:definition>
      <skos:prefLabel xml:lang="es">anual</skos:prefLabel>
      <skos:definition xml:lang="es">Frecuencia de un recurso publicado o actualizado una vez cada año.</skos:definition>
    </skos:Concept>
<!-- Concept: biennial  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/frequency/1014" xml:lang="en">
