<?xml version="1.0" encoding = "UTF-8"?>
<rdf:RDF
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:rdakit="http://metadataregistry.org/uri/profile/rdakit/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:reg="http://metadataregistry.org/uri/profile/regap/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: RDA Scale Designation -->
    <skos:ConceptScheme rdf:about="http://rdaregistry.info/termList/scale">
        <dc:title>RDA Scale Designation</dc:title>
    </skos:ConceptScheme>

<!-- Concept: not drawn to scale  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/scale/1004" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/scale"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/3392"/>
      <skos:prefLabel xml:lang="en">not drawn to scale</skos:prefLabel>
      <skos:definition xml:lang="en">A scale designation for a still image or three-dimensional form that is not to scale.</skos:definition>
      <rdakit:toolkitLabel xml:lang="en">not drawn to scale</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="en">A scale designation for a still image or three-dimensional form that is not to scale.</rdakit:toolkitDefinition>
    </skos:Concept>
<!-- Concept: scale not given  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/scale/1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/scale"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1142"/>
      <skos:prefLabel xml:lang="en">scale not given</skos:prefLabel>
      <skos:definition xml:lang="en">A scale designation for a resource when no scale can be determined.</skos:definition>
      <rdakit:toolkitLabel xml:lang="en">scale not given</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="en">A scale designation for a resource when no scale can be determined.</rdakit:toolkitDefinition>
    </skos:Concept>
<!-- Concept: scale varies  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/scale/1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/scale"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1143"/>
      <skos:prefLabel xml:lang="en">scale varies</skos:prefLabel>
      <skos:definition xml:lang="en">A scale designation for a resource whose scale is variable across the resource, when the range of values cannot be determined.</skos:definition>
      <rdakit:toolkitLabel xml:lang="en">scale varies</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="en">A scale designation for a resource whose scale is variable across the resource, when the range of values cannot be determined.</rdakit:toolkitDefinition>
    </skos:Concept>
<!-- Concept: scales differ  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/scale/1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/scale"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1141"/>
      <skos:prefLabel xml:lang="en">scales differ</skos:prefLabel>
      <skos:definition xml:lang="en">A scale designation for a resource consisting of more than one image, map, etc., with different scales.</skos:definition>
      <rdakit:toolkitLabel xml:lang="en">scales differ</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="en">A scale designation for a resource consisting of more than one image, map, etc., with different scales.</rdakit:toolkitDefinition>
    </skos:Concept>

<!-- Status properties used in this document  -->
    <skos:Concept rdf:about="http://metadataregistry.org/uri/RegStatus/1001">
        <skos:prefLabel xml:lang="en">Published</skos:prefLabel>
    </skos:Concept>

</rdf:RDF>
