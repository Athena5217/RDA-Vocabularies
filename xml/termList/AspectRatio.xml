<!-- Scheme: RDA Aspect Ratio -->
    <skos:ConceptScheme rdf:about="http://rdaregistry.info/termList/AspectRatio">
        <dc:title>RDA Aspect Ratio</dc:title>
    </skos:ConceptScheme>

<!-- Concept: full screen  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/AspectRatio/1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/AspectRatio"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/727"/>
      <skos:prefLabel xml:lang="en">full screen</skos:prefLabel>
      <skos:definition xml:lang="en">Aspect ratio for a moving image resource of less than 1.5:1.</skos:definition>
      <skos:prefLabel xml:lang="de">Vollbild</skos:prefLabel>
      <skos:prefLabel xml:lang="fr">plein écran</skos:prefLabel>
      <skos:definition xml:lang="de">Bildformat einer Bewegtbildressource von weniger als 1.5:1.</skos:definition>
      <skos:definition xml:lang="fr">Format de l’image d’une ressource d’images animées inférieur à 1,5:1.</skos:definition>
      <skos:prefLabel xml:lang="es">pantalla completa</skos:prefLabel>
      <skos:definition xml:lang="es">Proporción dimensional de un recurso de imagen en movimiento menor que 1.5:1.</skos:definition>
      <rdakit:ToolkitLabel xml:lang="en">full screen</rdakit:ToolkitLabel>
      <rdakit:ToolkitLabel xml:lang="fr">plein écran</rdakit:ToolkitLabel>
      <rdakit:ToolkitDefinition xml:lang="en">Aspect ratio for a moving image resource of less than 1.5:1.</rdakit:ToolkitDefinition>
      <rdakit:ToolkitDefinition xml:lang="fr">Format de l’image d’une ressource d’images animées inférieur à 1,5:1.</rdakit:ToolkitDefinition>
      <rdakit:ToolkitLabel xml:lang="de">Vollbild</rdakit:ToolkitLabel>
      <rdakit:ToolkitDefinition xml:lang="de">Bildformat einer Bewegtbildressource von weniger als 1.5:1.</rdakit:ToolkitDefinition>
      <rdakit:ToolkitLabel xml:lang="es">pantalla completa</rdakit:ToolkitLabel>
      <rdakit:ToolkitDefinition xml:lang="es">Proporción dimensional de un recurso de imagen en movimiento menor que 1.5:1.</rdakit:ToolkitDefinition>
      <skos:altLabel xml:lang="en">full-screen</skos:altLabel>
      <skos:altLabel xml:lang="en">fullscreen</skos:altLabel>
    </skos:Concept>
<!-- Concept: mixed  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/AspectRatio/1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/AspectRatio"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/729"/>
      <skos:prefLabel xml:lang="en">mixed</skos:prefLabel>
      <skos:definition xml:lang="en">Aspect ratio for a moving image resource that includes multiple aspect ratios within thesame resource.</skos:definition>
      <skos:prefLabel xml:lang="de">gemischt</skos:prefLabel>
      <skos:prefLabel xml:lang="fr">mixte</skos:prefLabel>
      <skos:definition xml:lang="de">Bildformat einer Bewegtbildressource, die mehrere Bildformate innerhalb derselben Ressource enthält.</skos:definition>
      <skos:definition xml:lang="fr">Format de l’image d’une ressource d’images animées comprenant divers formats de l’image à l’intérieur de la même ressource.</skos:definition>
      <skos:prefLabel xml:lang="es">mixta</skos:prefLabel>
      <rdakit:ToolkitLabel xml:lang="en">mixed</rdakit:ToolkitLabel>
      <rdakit:ToolkitLabel xml:lang="fr">mixte</rdakit:ToolkitLabel>
      <rdakit:ToolkitDefinition xml:lang="en">Aspect ratio for a moving image resource that includes multiple aspect ratios within thesame resource.</rdakit:ToolkitDefinition>
      <rdakit:ToolkitDefinition xml:lang="fr">Format de l’image d’une ressource d’images animées comprenant divers formats de l’image à l’intérieur de la même ressource.</rdakit:ToolkitDefinition>
      <rdakit:ToolkitLabel xml:lang="de">gemischt</rdakit:ToolkitLabel>
      <rdakit:ToolkitDefinition xml:lang="de">Bildformat einer Bewegtbildressource, die mehrere Bildformate innerhalb derselben Ressource enthält.</rdakit:ToolkitDefinition>
      <rdakit:ToolkitLabel xml:lang="es">mixta</rdakit:ToolkitLabel>
    </skos:Concept>
<!-- Concept: wide screen  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/AspectRatio/1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/AspectRatio"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/728"/>
      <skos:prefLabel xml:lang="en">wide screen</skos:prefLabel>
      <skos:definition xml:lang="en">Aspect ratio for a moving image resource of 1.5:1 or greater.</skos:definition>
      <skos:prefLabel xml:lang="de">Breitbild</skos:prefLabel>
      <skos:prefLabel xml:lang="fr">écran large</skos:prefLabel>
      <skos:definition xml:lang="de">Bildformat einer Bewegtbildressource von 1.5:1 oder höher.</skos:definition>
      <skos:prefLabel xml:lang="es">pantalla ancha</skos:prefLabel>
      <skos:definition xml:lang="fr">Format de l’image d’une ressource d’images animées égal ou supérieur à 1,5:1.</skos:definition>
      <rdakit:ToolkitLabel xml:lang="en">wide screen</rdakit:ToolkitLabel>
      <rdakit:ToolkitLabel xml:lang="fr">écran large</rdakit:ToolkitLabel>
      <rdakit:ToolkitDefinition xml:lang="en">Aspect ratio for a moving image resource of 1.5:1 or greater.</rdakit:ToolkitDefinition>
      <rdakit:ToolkitDefinition xml:lang="fr">Format de l’image d’une ressource d’images animées égal ou supérieur à 1,5:1.</rdakit:ToolkitDefinition>
      <rdakit:ToolkitLabel xml:lang="de">Breitbild</rdakit:ToolkitLabel>
      <rdakit:ToolkitDefinition xml:lang="de">Bildformat einer Bewegtbildressource von 1.5:1 oder höher.</rdakit:ToolkitDefinition>
      <rdakit:ToolkitLabel xml:lang="es">pantalla ancha</rdakit:ToolkitLabel>
      <skos:altLabel xml:lang="en">wide-screen</skos:altLabel>
      <skos:altLabel xml:lang="en">widescreen</skos:altLabel>
    </skos:Concept>

