<!-- Concept: 12mo  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/bookFormat/1004" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/bookFormat"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
			<reg:identifier rdf:resource="853"/>
      <skos:prefLabel xml:lang="en">12mo</skos:prefLabel>
      <skos:definition xml:lang="en">A book format consisting of one or more leaves that are 1/12 of the whole sheet.</skos:definition>
      <skos:altLabel xml:lang="en">duodecimo</skos:altLabel>
      <skos:altLabel xml:lang="en">twelvemo</skos:altLabel>
      <skos:definition xml:lang="fr">Format de livre constitué d’un ou plusieurs feuillets formant chacun 1/12 de la feuille complète.</skos:definition>
      <skos:altLabel xml:lang="fr">in-douze</skos:altLabel>
      <skos:prefLabel xml:lang="fr">12&amp;#186;</skos:prefLabel>
      <skos:definition xml:lang="de">Ein Buchformat, das aus einem oder mehreren Blättern besteht, das/die 1/12 des ganzen Bogens ist/sind.</skos:definition>
      <skos:altLabel xml:lang="de">Duodezimo</skos:altLabel>
      <skos:prefLabel xml:lang="de">12o</skos:prefLabel>
      <skos:altLabel xml:lang="es">doceavo</skos:altLabel>
      <skos:prefLabel xml:lang="es">12vo</skos:prefLabel>
      <skos:definition xml:lang="es">Un formato de libro que consiste en una o más hojas que son 1/12 del folio completo.</skos:definition>
    </skos:Concept>
