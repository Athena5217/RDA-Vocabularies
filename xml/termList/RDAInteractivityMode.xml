<?xml version="1.0" encoding = "UTF-8"?>
<rdf:RDF
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:rdakit="http://metadataregistry.org/uri/profile/rdakit/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:reg="http://metadataregistry.org/uri/profile/regap/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: RDA Interactivity Mode -->
    <skos:ConceptScheme rdf:about="http://rdaregistry.info/termList/RDAInteractivityMode">
        <dc:title>RDA Interactivity Mode</dc:title>
    </skos:ConceptScheme>

<!-- Concept: interactive  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/RDAInteractivityMode/1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/RDAInteractivityMode"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/12584"/>
      <skos:prefLabel xml:lang="en">interactive</skos:prefLabel>
      <skos:notation xml:lang="en">1001</skos:notation>
      <rdakit:toolkitLabel xml:lang="en">interactive</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">An interactivity mode that indicates that the content of an expression responds to actions performed by the user.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">An interactivity mode that indicates that the content of an expression responds to actions performed by the user.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">Actions include searching, using commands, and making selections.</skos:scopeNote>
    </skos:Concept>
<!-- Concept: non-interactive  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/RDAInteractivityMode/1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/RDAInteractivityMode"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/12585"/>
      <skos:prefLabel xml:lang="en">non-interactive</skos:prefLabel>
      <skos:notation xml:lang="en">1002</skos:notation>
      <rdakit:toolkitLabel xml:lang="en">non-interactive</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">An interactivity mode that indicates that the content of an expression does not respond to actions performed by the user.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">An interactivity mode that indicates that the content of an expression does not respond to actions performed by the user.</rdakit:toolkitDefinition>
    </skos:Concept>

<!-- Status properties used in this document  -->
    <skos:Concept rdf:about="http://metadataregistry.org/uri/RegStatus/1001">
        <skos:prefLabel xml:lang="en">Published</skos:prefLabel>
    </skos:Concept>

</rdf:RDF>
