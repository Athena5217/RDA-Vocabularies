<?xml version="1.0" encoding = "UTF-8"?>
<rdf:RDF
    xmlns="http://www.w3.org/2004/02/skos/core#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:reg="http://metadataregistry.org/uri/schema/registry/">

<!-- Scheme: RDA Status of Identification -->
    <skos:ConceptScheme rdf:about="http://rdaregistry.info/termList/statIdentification">
        <dc:title>RDA Status of Identification</dc:title>
    </skos:ConceptScheme>

<!-- Concept: fully established  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/statIdentification/1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/statIdentification"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
			<reg:identifier rdf:resource="1008"/>
      <skos:prefLabel xml:lang="en">fully established</skos:prefLabel>
      <skos:definition xml:lang="en">Status of identification for an authorized access point when the data is complete.</skos:definition>
      <skos:prefLabel xml:lang="de">vollständig etabliert</skos:prefLabel>
      <skos:prefLabel xml:lang="fr">complètement établi </skos:prefLabel>
      <skos:definition xml:lang="de">Status der Identifizierung für den normierten Sucheinstieg, wenn die Daten vollständig sind.</skos:definition>
      <skos:definition xml:lang="fr">Statut de l’identification d’un point d’accès autorisé lorsque les données sont complètes.</skos:definition>
    </skos:Concept>
<!-- Concept: preliminary  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/statIdentification/1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/statIdentification"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
			<reg:identifier rdf:resource="1010"/>
      <skos:prefLabel xml:lang="en">preliminary</skos:prefLabel>
      <skos:definition xml:lang="en">Status of identification for an authorized access point when the data is taken from a description without the resource described in hand.</skos:definition>
      <skos:prefLabel xml:lang="de">vorläufig</skos:prefLabel>
      <skos:prefLabel xml:lang="fr">préliminaire</skos:prefLabel>
      <skos:definition xml:lang="de">Status der Identifizierung für den normierten Sucheinstieg, wenn die Daten aus einer Beschreibung entnommen sind, ohne dass die beschriebene Ressource vorliegt.</skos:definition>
      <skos:definition xml:lang="fr">Statut de l’identification d’un point d’accès autorisé lorsque les données sont tirées d’une description faite sans avoir la ressource décrite en main.</skos:definition>
    </skos:Concept>
<!-- Concept: provisional  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/statIdentification/1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/statIdentification"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
			<reg:identifier rdf:resource="1009"/>
      <skos:prefLabel xml:lang="en">provisional</skos:prefLabel>
      <skos:definition xml:lang="en">Status of identification for an authorized access point when the data is insufficient to establish an authorized access point representing the entity.</skos:definition>
      <skos:prefLabel xml:lang="de">provisorisch</skos:prefLabel>
      <skos:prefLabel xml:lang="fr">provisoire</skos:prefLabel>
      <skos:definition xml:lang="de">Status der Identifizierung für einen Sucheinstieg, wenn die Daten nicht ausreichen, um einen normierten Sucheinstieg zu etablieren, der die Entität repräsentiert.</skos:definition>
      <skos:definition xml:lang="fr">Statut de l’identification d’un point d’accès lorsque les données sont insuffisantes pour établir un point d’accès autorisé représentant une entité.</skos:definition>
    </skos:Concept>

<!-- Status properties used in this document  -->
    <skos:Concept rdf:about="http://metadataregistry.org/uri/RegStatus/1001">
        <skos:prefLabel xml:lang="en">Published</skos:prefLabel>
    </skos:Concept>

</rdf:RDF>