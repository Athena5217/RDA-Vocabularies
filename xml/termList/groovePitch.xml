<!-- Scheme: RDA Groove Pitch of an Analog Cylinder -->
    <skos:ConceptScheme rdf:about="http://rdaregistry.info/termList/groovePitch">
        <dc:title>RDA Groove Pitch of an Analog Cylinder</dc:title>
    </skos:ConceptScheme>

<!-- Concept: fine  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/groovePitch/1005" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/groovePitch"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
			<reg:identifier rdf:resource="870"/>
      <skos:prefLabel xml:lang="en">fine</skos:prefLabel>
      <skos:definition xml:lang="en">Groove characteristic for an analog cylinder with a groove pitch in the range of 60-64 turns per cm (150 or 160 turns per inch) for 6 1/8 inch dictation format cylinders, or 200 TPI for “4 minute” cylinders.</skos:definition>
    </skos:Concept>
<!-- Concept: standard  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/groovePitch/1006" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/groovePitch"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
			<reg:identifier rdf:resource="871"/>
      <skos:prefLabel xml:lang="en">standard</skos:prefLabel>
      <skos:definition xml:lang="en">Groove characteristic for an analog cylinder with a groove pitch of 40 turns per cm (100 turns per inch).</skos:definition>
    </skos:Concept>

