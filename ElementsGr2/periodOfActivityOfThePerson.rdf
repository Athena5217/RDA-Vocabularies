<!--Property: Period of activity of the person-->
<rdf:Description rdf:about="http://rdvocab.info/ElementsGr2/periodOfActivityOfThePerson">
  <rdfs:isDefinedBy rdf:resource="http://rdvocab.info/ElementsGr2" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1002" />
  <reg:name xml:lang="en">periodOfActivityOfThePerson</reg:name>
  <rdfs:label xml:lang="en">Period of activity of the person</rdfs:label>
  <skos:definition xml:lang="en">A date or range of dates indicative of the period in which a person was active in his or her primary field of endeavour.</skos:definition>
