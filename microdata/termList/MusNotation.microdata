    <meta itemprop="definition" content="Notation musicale qui utilise les lettres de l’alphabet pour désigner les hauteurs de sons." />
    <link itemprop="http://metadataregistry.org/uri/schema/registry/status" href="http://metadataregistry.org/uri/RegStatus/1001" />
