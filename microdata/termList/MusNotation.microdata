    <link itemprop="http://metadataregistry.org/uri/schema/registry/status" href="http://metadataregistry.org/uri/RegStatus/1001" />
    <meta itemprop="definition" content="Die Kennzeichnung von Tonhöhen durch konventionelle Silben anstelle von Buchstabenbezeichnungen " />
    <link itemprop="inScheme" href="http://rdaregistry.info/termList/MusNotation" />
    <meta itemprop="definition" content="La désignation des hauteurs de sons par des syllabes de convention plutôt que par des lettres." />
