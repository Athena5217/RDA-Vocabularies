    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "lang": "fi", 
        "type": "literal", 
        "value": "Elokuvafilmin esitysformaatti, jonka kuvasuhde on enintään 2.76:1 ja joka käyttää alun perin kolmea synkronoitua projektoria."
      }, 
      {
        "lang": "en", 
        "type": "literal", 
        "value": "A presentation format of a motion picture film that presents an aspect ratio of up to 2.76:1 that originally used three synchronized projectors."
      }, 
      {
        "lang": "vi", 
        "type": "literal", 
        "value": "Định dạng trình bày của phim điện ảnh trình bày tỷ lệ bề ngoài lên tới 2,78:1, sử dụng nguyên gốc ba máy chiếu đồng bộ."
      }, 
      {
        "lang": "fr", 
        "type": "literal", 
        "value": "Format de projection d’un film cinématographique qui produit un format d’image allant jusqu’à 2,76:1 et qui utlisait à l'origine trois projecteurs synchronisés."
      }, 
      {
        "lang": "hu", 
        "type": "literal", 
        "value": "A mozgófilm olyan bemutatási formátuma, amely  eredetileg három, szinkronizált vetítő használatával, maximálisan 2,76:1 képméretaránnyal dolgozik."
      }, 
      {
        "lang": "de", 
        "type": "literal", 
        "value": "Ein Wiedergabeformat eines Films, das ein Bildformat von bis zu 2,76:1 darstellt, für das ursprünglich drei synchron laufende Projektoren verwendet wurden."
      }, 
      {
        "lang": "da", 
        "type": "literal", 
        "value": "Præsentationsformat til film, som har et billedformat på op til 2.76:1, og som oprindelig anvendte tre synkroniserede projektorer."
      }, 
      {
        "lang": "ca", 
        "type": "literal", 
        "value": "Format de presentació d'una pel·lícula cinematogràfica que presenta una relació d'aspecte fins a 2,76:1, que originalment usava tres projectors sincronitzats."
      }
    ], 
