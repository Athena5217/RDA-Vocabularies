    skos:prefLabel "leather"@en ;
    skos:related <http://rdaregistry.info/termList/RDAbaseMaterial/1017> .

<http://rdaregistry.info/termList/RDAbaseMaterial/1021> a skos:Concept ;
    reg:identifier <458> ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    skos:definition "A base material consisting of fine-quality calf or lamb parchment."@en ;
    skos:inScheme <http://rdaregistry.info/termList/RDAbaseMaterial> ;
    skos:prefLabel "vellum"@en ;
    skos:related <http://rdaregistry.info/termList/RDAbaseMaterial/1012> .

<http://rdaregistry.info/termList/RDAbaseMaterial/1012> a skos:Concept ;
    reg:identifier <449> ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    skos:definition "A base material consisting of calf, sheep, or goat skin which has been prepared to produce a thin, strong, translucent or opaque substance for writing, bookbinding, or other uses."@en ;
    skos:inScheme <http://rdaregistry.info/termList/RDAbaseMaterial> ;
    skos:prefLabel "parchment"@en ;
    skos:related <http://rdaregistry.info/termList/RDAbaseMaterial/1017>,
        <http://rdaregistry.info/termList/RDAbaseMaterial/1021> .

<http://rdaregistry.info/termList/RDAbaseMaterial/1017> a skos:Concept ;
