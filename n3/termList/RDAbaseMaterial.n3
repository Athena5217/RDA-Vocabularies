    skos:inScheme <http://rdaregistry.info/termList/RDAbaseMaterial> ;
    skos:prefLabel "skin"@en ;
    skos:related <http://rdaregistry.info/termList/RDAbaseMaterial/1009>,
        <http://rdaregistry.info/termList/RDAbaseMaterial/1012> .

